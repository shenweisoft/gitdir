<!-- wetoast.wxml -->
<template name="wetoast">
  <view wx:if="{{toastType == 'error'}}" class='top_title1'>{{content}}</view>
  <view wx:if="{{toastType == 'success'}}" class='top_title2'>{{content}}</view>
</template>
<!-- 底部菜单 -->
<template name="footInfo">
  <view class='bottom_btn_box'>
    <!-- 步数提示框 -->
    <!-- <view class='progress_title'style='left:{{currentStep/5 * 100}}%;'>
      {{currentStep}}/5
      <view class='progress_title_b'></view>
    </view> -->
    <progress percent="{{currentStep/5 * 100}}" />
     <!-- <view class='nextBtn' wx:if="{{currentStep==1}}" data-step="{{currentStep}}" bindtap='nextStep'><view class='nextBtn_c'>下一步</view></view>  -->
    <view class='nextBtn' wx:if="{{currentStep != '5'}}">
      <view class='left_btn' bindtap='preStep'>上一步</view>
      <view class='right_btn' bindtap='nextStep'>下一步</view>
      <view class='clear'></view>
    </view>
    <view class='nextBtn' wx:if="{{currentStep=='5'}}">
      <view class='left_btn' bindtap='reservation'><icon class='iconfont icon--yuyuetiaojie' style='font-size:20px; position: relative; top: 1px;'></icon> 预约调解</view>
      <!-- <view class='right_btn'><icon class='iconfont icon--fenxiang' style='font-size:20px; position: relative; top: 1px;'></icon> 分享</view> -->
      <button class='right_btn' open-type="share">
        <icon class='iconfont icon--fenxiang' style='font-size:20px; position: relative; top: 1px;'></icon> 分享
      </button>
      <view class='clear'></view>
    </view>
  </view>
</template>
<template name='lawCaseHeader'>
  <view class='top_step_list'>
    <view class='top_step_list_e' wx:for="{{stepArray}}" wx:for-item="x" wx:key="{{x.id}}">
      <view class="{{step >= x.id? ((step == x.id)? 'step active' : 'step active2'): 'step'}}">{{x.id}}<icon class='iconfont icon-dui'></icon></view>
      {{x.name}}
    </view>
  </view>
</template>

<template name='adjustAgreement'>
  <view class="b1 b2 wxParse-body wxParse-p">
    <view class="wxParse-p" style="text-align:center; width:100%;font-family:'宋体'; font-size: 34px;font-weight: bold;">
      <view class="WxEmojiView wxParse-inline" style='font-size:45rpx'>人 民 调 解 协 议 书</view>
    </view>
    <view class="wxParse-p" style="width: 100%; text-align: right; font-family: '仿宋_GB2312'; font-size: 20px; margin-bottom: 25px;">
      <view class="ng-binding wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline"> {{lawCase.regulationNo}}号</view>
      </view>
    </view>
    <!-- 当事人数据 -->
    <view class="ng-scope wxParse-div" wx:for="{{lawCase.applicantArray}}">
      <view class="wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
        <view class="ng-scope ng-binding wxParse-span wxParse-inline">
          <view class="WxEmojiView wxParse-inline">当事人：
          <view class="wxParse-span wxParse-inline">{{item.idType == 0? item.personName : item.legalName}}，</view>
          <view class="wxParse-span wxParse-inline" wx:if='{{item.sex || item.sex == "0"}}'>{{item.sex == 0? '男':'女'}}，</view>
          
          <view class="wxParse-span wxParse-inline" wx:if='{{item.birthDay}}'>{{item.birthDayStr}}出生，</view>
          <view class="wxParse-span wxParse-inline" wx:if='{{item.nation}}'>{{item.nation}}，</view>
          <view class="wxParse-span wxParse-inline" wx:if='{{item.idNo && (item.certificatesType || item.certificatesType == "0") && item.certificatesType != 5}}'>{{item.certificatesStr}}号码：{{item.idNo}}，</view>
          住{{item.domicile}}
          </view>

              <view wx:if="{{i.agentArray}}">
                  <view  wx:for="{{i.agentArray}}" wx:for-item="m">
                    <view wx:if="{{m.agentType == '0'}}"><!--委托-->
                      <view class="WxEmojiView" style="text-indent:30px;" wx:if="{{m.agentIdentity == '1'}} || {{m.agentIdentity == '0'}} || {{m.agentIdentity == '2'}}">
                        委托代理人：{{m.agentName}},
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男，'}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生,</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '1'}}">驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '2'}}">外籍在华驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '3'}}">军队驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '4'}}">律师证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '5'}}">暂未获取</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.companyName}}">{{m.companyName}}职员</view>
                        ，住{{i.sendAddress}}
                      </view>
                    </view>
                    <view class="WxEmojiView" wx:if="{{m.agentType != '0'}}" style="text-indent:30px;">法定代理人：
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男，'}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生,</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}},住</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.isLive==true}}">{{m.residence}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.isLive==false}}">{{m.domicile}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{i.idType == '0'}}">（系申请人{{i.personName}}的</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{i.idType != '0'}}">（系申请人{{i.legalName}}的</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '1'}}">父亲</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '2'}}">母亲</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '3'}}">儿子</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '4'}}">女儿</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '5'}}">配偶</view>)
                    </view>
                  </view>
               </view>

        </view>
      </view>
    </view>
    <!-- 纠纷类别 -->
    <view class="wxParse-p" wx:if='{{lawCase.reason || lawCase.reason == 0}}' style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="ng-scope ng-binding wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">纠纷类别：{{lawCase.reasonStr}}</view>
      </view>
    </view>
    <!-- 纠纷事实 -->
    <view wx:if='{{lawCase.factReason}}' class="ng-binding wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline"> 纠纷主要事实，争议事项：{{lawCase.factReason}}</view>
    </view>
    <!-- 赔偿明细 -->
    <view class="ng-binding wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline"> 当事人{{lawCase.applicant}}因交通事故损失的<view class="wxParse-span wxParse-inline" wx:for='{{lawCase.feeDetail}}' wx:if='{{item.claimAmount}}'>{{item.value}}{{item.claimAmount}}元，</view>合计{{lawCase.lawMoney}}元，要求当事人{{lawCase.respondent}} 赔偿。</view>
    </view>

    <view class="wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline"> 经调解，自愿达成如下协议：</view>
    </view>
    <view class="ng-scope ng-binding wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';">
      <view wx:for='{{lawCase.adjustResultRemarkStr}}' class="wxParse-p" style="font-family: '仿宋_GB2312';">{{item}}</view>
    </view>
    
    <view class="wxParse-p" style="text-indent: 40px; font-size: 20px; font-family: '仿宋_GB2312';margin-bottom:40rpx;"
>
      <view class="WxEmojiView wxParse-inline"> 本协议一式_____份，当事人、人民调解委员会各持一份。</view>
    </view>
    
    <view wx:for='{{lawCase.applicantArray}}' style='height:151rpx' wx:for-item='applicant' wx:for-index='index' wx:if='{{applicant.personType == "0"}}'>
      <view class='clear sign-wp'>
        <view data-index='{{index}}' class='signImage' wx:if="{{!applicant.isSignature && ((!isLocalSignature && document.remoteSign != 1) || (isLocalSignature && userInfo.userType != 2) || (document.remoteSign == 1 && userInfoType != '0') || lawCase.state != '1000')}}"></view>
        <view data-index='{{index}}' bindtap='handleSignature' class='signImage' wx:if="{{!applicant.isSignature && lawCase.state == '1000' && ((isLocalSignature && userInfo.userType == 2) || (document.remoteSign == 1 && userInfoType == '0'))}}">点我签字</view>
        <image class='signImage' wx:for='{{document.signatureList}}' src='{{imageAddress+item.signature}}' wx:if='{{applicant.id == item.applicantId}}'></image>
        <view class='signName'>申请人签字：</view>
      </view>
    </view>
    <view wx:for='{{lawCase.applicantArray}}' style='height:151rpx' wx:for-item='respondent' wx:for-index='index' wx:if='{{respondent.personType == 1}}'>
      <view class='clear sign-wp'>
        <view data-index='{{index}}' class='signImage' wx:if="{{!respondent.isSignature && ((!isLocalSignature && document.remoteSign != 1) || (isLocalSignature && userInfo.userType != 2) || (document.remoteSign == 1 && userInfoType != 1) || lawCase.state != '1000')}}"></view>
        <view data-index='{{index}}' bindtap='handleSignature' class='signImage' wx:if="{{!respondent.isSignature && lawCase.state == '1000' && ((isLocalSignature && userInfo.userType == 2) || (document.remoteSign == 1 && userInfoType == 1))}}">点我签字</view>
        <image class='signImage' wx:for='{{document.signatureList}}' src='{{imageAddress+item.signature}}' wx:if='{{respondent.id == item.applicantId}}'></image>
        <view class='signName'>被申请人签字：</view>
      </view>
    </view>
    <view style='height:151rpx'>
      <view class='clear sign-wp'>
        <view data-index='{{index}}' class='signImage' wx:if="{{(!isAdjustSignature && userInfo.userType != 2) || (lawCase.state != '1000' && !isAdjustSignature && userInfo.userType == 2)}}"></view>
        <view bindtap='handleSignature' class='signImage' data-index='{{index}}' wx:if="{{!userInfo.isSignature && userInfo.userType == 2 && lawCase.state == '1000'}}">点我签字</view>
        <image class='signImage' wx:for='{{document.signatureList}}' wx:if='{{lawCase.adjustPersonId == item.applicantId}}' src='{{imageAddress+item.signature}}'></image>
        <view class='signName'>调解员签字：</view>
      </view>
    </view>
    
    <!-- <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">当事人（签名盖章或按指印）:_______人民调解员（签名）_____</view>
      </view>
    </view>
    
    <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">当事人（签名盖章或按指印）:_______记录人（签名）______</view>
      </view>
    </view> -->
    
    <view class="wxParse-p" style="text-align: right; padding-right: 50px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%; width: 100%;">
      <view class="wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">{{lawCase.adjustOrgName}}</view>
      </view>
    </view>
    <view class="wxParse-p" style="text-align: right; padding-right: 50px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%; width: 100%;">
      <view class="ng-binding wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">{{lawCase.upperTodata}}</view>
      </view>
    </view>
    
    <view class="wxParse-p" style="text-align:center; width:100%;font-family:'仿宋_GB2312'; font-size: 34px;font-weight: bold;">
      <view class="WxEmojiView wxParse-inline">调 解 说 明</view>
    </view>
    <view class="ng-binding wxParse-p" style="text-indent: 28px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline">{{lawCase.factReason}}</view>
    </view>
    <view class="ng-binding wxParse-p" style="text-indent: 28px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline">当事人{{lawCase.applicant}}与当事人{{lawCase.respondent}}是{{lawCase.reasonStr}}一案，该纠纷于{{lawCase.today}}{{lawCase.adjustOrgName}}调解，当事人之间达成协议如下：</view>
    </view>
    <view class="wxParse-p" style="text-indent: 28px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;margin-top:40rpx;"
>
      <view class="wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">费用明细：</view>
      </view>  
    </view>

    <view class="t1 wxParse-table" style="position: relative; left: 28px; width: 100%;border-collapse:collapse;">
      <view class="wxParse-tbody">
        <view class="r1 wxParse-tr" style="border-collapse: collapse; ">
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%;overflow:hidden;">
            <view class="WxEmojiView wxParse-inline">序号</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">费用名称</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">申请金额</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">调解金额</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">备注说明</view>
          </view>
        </view>
        <view class="ng-scope wxParse-tr" style='margin-top:-3px;' wx:for='{{lawCase.feeDetail}}' wx:if='{{item.claimAmount}}' wx:for-index='index'>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">{{index+1}}</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">{{item.value}}</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">{{item.applyAmount}}</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">{{item.claimAmount}}</view>
          </view>
          <view class="wxParse-td" style="border:1px solid #333; text-align: center; width: 8%">
            <view class="WxEmojiView wxParse-inline">{{item.remark}}</view>
          </view>
        </view>
      </view>
    </view>
    <text></text>
    <text></text>
    <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;margin-top:40rpx;">
      <view class="WxEmojiView wxParse-inline" style='width:100%;overflow:hidden;'> 备注：_____________________________________________</view>
    </view>
    <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline" style='width:100%;overflow:hidden;'>  ___________________________________________________</view>
    </view>
    <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:125%;">
      <view class="WxEmojiView wxParse-inline" style='width:100%;overflow:hidden;'>  ___________________________________________________</view>
    </view>
    <text></text>
    <view class="wxParse-p" style=" font-size: 20px; font-family: '仿宋_GB2312';line-height:400%; width: 100%; text-align: right; ">
      <view class="wxParse-span wxParse-inline" style='position:relative;top:-9rpx;'>
        <view class="WxEmojiView wxParse-inline">调解员（签名）:</view>
      </view>
      <view class="wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">
          <view bindtap='handleSignature' class='signImage' data-index='{{index}}' wx:if="{{!userInfo.isSignature && userInfo.userType == 2 && lawCase.state == '1000'}}">点我签字</view>
          <image class='signImage' wx:for='{{document.signatureList}}' wx:if='{{lawCase.adjustPersonId == item.applicantId}}' src='{{imageAddress+item.signature}}'></image>
        </view>
      </view>
      <view class="wxParse-span wxParse-inline" wx:if="{{(!isAdjustSignature && userInfo.userType != 2) || (lawCase.state != '1000' && !isAdjustSignature && userInfo.userType == 2)}}">
        <view data-index='{{index}}' class='signImage'></view>
      </view>

    </view>
    <view class="wxParse-p" style="text-align: right; padding-right: 50px; font-size: 20px; font-family: '仿宋_GB2312';line-height:125%; width: 100%;">
      <view class="ng-binding wxParse-span wxParse-inline">
        <view class="WxEmojiView wxParse-inline">{{lawCase.today}}</view>
      </view>
    </view>
  </view>
</template>

<!--调解笔录-->
<template name='mediationRecord'>
  <view class="wxParse">
        <view class="wxParse-body wxParse-"style="tab-interval:21.0pt">
            <view class="Section1 wxParse-div">
                <view class="p1 wxParse-p" style="font-size:16.0pt;text-align:center;font-weight:blod;">调解笔录</view>
                <view class="p2 wxParse-p">
                    <view class="WxEmojiView wxParse-inline">时间：{{lawCase.adjustDate}}日</view></view>
                <view class="p2 wxParse-p">
                    <view class="WxEmojiView wxParse-inline">地点：{{lawCase.adjustPointName}}</view>
                </view>
                <view class="p2 wxParse-p">
                    <view class="WxEmojiView wxParse-inline">调解员：{{lawCase.adjustName}}</view>
                </view>
                <view class="wxParse-p" wx:for="{{lawCase.applicantArray}}" wx:for-item="i" >
                   <view style="text-indent:30px;" wx:if="{{i.idType=='0'}}">{{i.personType=='0'? '申请人':'被申请人'}}{{i.no}}：{{i.personName}},{{i.sex=='1'? '女':'男，'}}{{i.birthDay}}日出生,{{i.nation}}，身份证号码：{{i.idNo}}，住
                  {{i.domicile}}</view>
                  <view style="text-indent:30px;" wx:if="{{i.idType != '0'}}">{{i.personType=='0'? '申请人':'被申请人'}}{{i.no}}：{{i.orgName}},住所地：{{i.residence}}
                </view>

              
                <view style="text-indent:30px;" wx:if="{{i.idType != '0'}}">{{i.legalType=='1'? '主要负责人':'法定代表人'}}{{i.no}}：{{i.legalName}},{{i.orgName}}{{i.job}}
                </view>
                <view wx:if="{{i.agentArray}}">
                  <view wx:for="{{i.agentArray}}" wx:for-item="m">
                    <view wx:if="{{m.agentType == '0'}}"><!--委托-->
                      <view class="WxEmojiView" style="text-indent:30px;" wx:if="{{m.agentIdentity == '1'}} || {{m.agentIdentity == '0'}} || {{m.agentIdentity == '2'}}">
                        委托代理人{{m.no}}：{{m.agentName}},
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男，'}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生,</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '1'}}">驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '2'}}">外籍在华驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '3'}}">军队驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '4'}}">律师证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '5'}}">暂未获取</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.companyName}}">{{m.companyName}}职员</view>
                        ，住{{i.sendAddress}}
                      </view>
                    </view>
                    <view class="WxEmojiView" wx:if="{{m.agentType != '0'}}" style="text-indent:30px;">法定代理人{{m.no}}：
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男，'}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生,</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}},住</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.isLive==true}}">{{m.residence}}</view>
