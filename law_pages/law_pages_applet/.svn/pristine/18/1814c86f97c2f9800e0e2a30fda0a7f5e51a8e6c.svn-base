<template name='civilLitigation'>
  <view class="wxParse">
    <view class="wxParse-body wxParse-" style="tab-interval:21.0pt">
      <view class="Section1 wxParse-div">
        <view class="p1 wxParse-p" style="font-size:16.0pt;text-align:center;font-weight:blod;">民事起诉状</view>
        <view class="wxParse-p" wx:for="{{lawCase.applicantArray}}" wx:for-item="i" >
          <view class="wxParse-p p1" wx-if="{{i.idType=='0'}}">{{i.personType=='0'? '原告：':'被告：'}}{{i.personName}}，{{i.sex=='1'? '女':'男'}}，{{i.birthDay}}日出生，{{i.nation}}，身份证号码：{{i.idNo}}，住{{i.domicile}}</view>

          <view style="text-indent:30px;" wx:if="{{i.idType != '0'}}">{{i.legalType=='1'? '主要负责人':'法定代表人'}}：{{i.legalName}}，{{i.orgName}}{{i.job}}
                </view>
                <view wx:if="{{i.agentArray}}">
                  <view  wx:for="{{i.agentArray}}" wx:for-item="m">
                    <view wx:if="{{m.agentType == '0'}}"><!--委托-->
                      <view class="WxEmojiView" style="text-indent:30px;" wx:if="{{m.agentIdentity == '1'}} || {{m.agentIdentity == '0'}} || {{m.agentIdentity == '2'}}">
                        委托代理人：{{m.agentName}}，
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男，'}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生，</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}}</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '1'}}">驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '2'}}">外籍在华驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '3'}}">军队驾驶证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '4'}}">律师证</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.certificatesType == '5'}}">暂未获取</view>
                        <view class="WxEmojiView wxParse-inline" wx:if="{{m.companyName}}">{{m.companyName}}职员</view>
                        ，住{{i.sendAddress}}
                      </view>
                    </view>
                    <view class="WxEmojiView" wx:if="{{m.agentType != '0'}}" style="text-indent:30px;">法定代理人：
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.sex}}">{{m.sex=='1'? '女':'男'}}，</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.birthDay}}">{{m.birthDay}}日出生，</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.nation}}">{{m.nation}}，</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.idNo}}">身份证号码：{{m.idNo}},住</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.isLive==true}}">{{m.residence}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.isLive==false}}">{{m.domicile}}</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{i.idType == '0'}}">（系申请人{{i.personName}}的</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{i.idType != '0'}}">（系申请人{{i.legalName}}的</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '1'}}">父亲</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '2'}}">母亲</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '3'}}">儿子</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '4'}}">女儿</view>
                      <view class="WxEmojiView wxParse-inline" wx:if="{{m.relation == '5'}}">配偶</view>)
                    </view>
                  </view>
               </view>

        </view>
        <view wx-if="{{i.idType=='1' || i.idType=='2'}}">
          {{i.personType=='0'? '原告：':'被告：'}}{{lawCase.respondent}},住{{i.domicile}}
        </view>
        <view class="wxParse-p">
          <view class="WxEmojiView" style="text-indent:30px;">诉讼请求：请求法院判令{{lawCase.respondent}}支付原告
            <text wx:for="{{lawCase.feeDetail}}" wx:for-item="i">
              <text wx-if="{{i.claimAmount>0}}">{{i.value}}{{i.claimAmount}}元，</text>
            </text>
            <view class="WxEmojiView wxParse-inline">合计{{lawCase.applyTotal}}元。</view>
          </view>
        </view>
        <view class=" wxParse-p"  style="text-indent:30px;">
          <view class="WxEmojiView wxParse-inline">事实与理由：{{lawCase.factReason}}</view>
        </view>
        <view class="pwxParse-p" style="text-align:right;">
          <!-- <view class="WxEmojiView wxParse-inline">具状人：{{lawCase.applicant}}</view> -->
        </view>
        <view class="pwxParse-p" style="text-align:right;">
          <view class="WxEmojiView wxParse-inline">{{lawCase.adjustDate}}日</view>
        </view>
      </view>
    </view>
  </view>
</template> 

