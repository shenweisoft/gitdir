.detail_bg{
  background: #ebf3f6;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
  top: 0px;
}

.page{
  background: #ffffff;
  padding: 0 30rpx 0 30rpx; 
  color: #fff; 
}
.searchInput{
  color:#666;
  border-radius: 16rpx;
  text-indent: 10rpx;
  margin:10px 0px;
  padding: 10px;
  font-size: 16px;
}
.title{
  text-indent:20rpx; 
  color: #000;
  line-height: 35px;
  border-bottom: 1px solid #dddddd;
}
.list{
  border-bottom: 1px solid #dddddd;
  padding:20rpx 10rpx 20rpx 20rpx;
}
.list:last-child{
  border: 0;
}
.image{
  width:50px; 
  height:50px; 
  vertical-align: middle;
  margin-left: 10px;
  margin-right: 10px;
}
.btn{
  color:#FFF; 
  background-color:#2196f3;
  width: 650rpx;
  position: fixed;
  bottom: 10px;
  left: 50rpx
}
.group{
  background: #ffffff;
  padding: 10rpx 35rpx;
  margin-bottom: 60px;
}