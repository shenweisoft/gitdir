    overflow: auto;
}*/

.wxParse-u {
  text-decoration: underline;
}
.wxParse-hide{
    display: none;
}
.WxEmojiView{
    align-items: center;
}
.wxEmoji{
    width: 16px;
    height:16px;
}
.wxParse-tr{
	display: flex;
	border-right:1px solid #e0e0e0;
	border-bottom:1px solid #e0e0e0;
	border-top:1px solid #e0e0e0;
}
.wxParse-th,
.wxParse-td{
	flex:1;
	padding:5px;
	font-size:28rpx;
	border-left:1px solid #e0e0e0;
	word-break: break-all;
}
.wxParse-td:last{
	border-top:1px solid #e0e0e0;
}
.wxParse-th{
	background:#f0f0f0;
	border-top:1px solid #e0e0e0;
}
.wxParse-del{
    display: inline;
}
.wxParse-figure {
  overflow: hidden;
}
.text-title{
  text-align: center;
  font-weight: bold;
  color:#333;
  font-size:16px;
}
.document_t{
  text-align: center;
  font-size: 18rpx;
  line-height: 50rpx;
}
.document_c{
  padding: 0rpx 16rpx;
  font-size: 15rpx;
  color: #656565;
  line-height: 30rpx;
}
.wxParse view{
 font-size:14px;
}

/* .wxParse view{
  font-size:14px !important;
} */

.submit_btn_box{
  width: 750rpx;
  background: #ffffff;
  border-top: 15rpx solid #f2f2f2;
  position: fixed;
  bottom: 0rpx;
  padding-bottom:10px;
  padding-top: 10px;
}
.submit_btn{
  width: 690rpx;
  height: 40px;
  font-size: 15px;
  text-align: center;
  line-height: 40px;
  background: #0e9deb;
  color: #ffffff;
  border-radius: 4px;
  margin-left: 30rpx;
  margin-top: 10rpx;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  background: #000000;
  opacity: 0.3;
  z-index: 99999;
}
.th_alert{
  width: 600rpx;
  height: 500rpx;
  background: #ffffff;
  position: fixed;
  top: 100rpx;
  left: 75rpx;
  z-index: 999999;
  border-radius: 5rpx;
}
.th_alert .bt{
  text-align: center;
  line-height: 50rpx;
  color: #999999;
  font-size: 16px;
  margin-top: 10px;
}
.th_alert .text{
  font-size: 15rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  text-indent: 30rpx;
  color: #666666;
}
.p1,.p2,.p3,.p4,.p5{
  text-indent: 30px;
}
.fl{
  width:50%;
  float: left;
}
.btn{
  width: 150px;
  height: 40px;
  color: #ffffff;
  background: #0e9deb;
  text-align: center;
  line-height: 40px;
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  margin-left: -75px;
  border-radius: 4px;
  font-size: 16px;
}
.btn1{
  width: 500rpx;
  margin-left: 50rpx;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 15px;
  background: #e7e7e7;
  margin-top: 20px;
  border-radius: 4px;
}

.btn1:hover{
  width: 500rpx;
  margin-left: 50rpx;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 15px;
  background: #d3f0fe;
  margin-top: 20px;
  border-radius: 4px;
}
.signImage {
  width: 250rpx;
  height: 150rpx;
  float: right;
  text-align: center;
  line-height: 150rpx;
  color:#0e9deb;
} 
.signName {
  font-size:15rpx;
  line-height:30rpx;
  float: right;
  position: relative;
  top: 60rpx;
}
.clear:after {
  display: block;
  clear: both;
  content: '';
}
.sign-wp{
  margin-bottom: 20rpx;
}
.wxParse-table{
  width: 90% !important;
  overflow: hidden;
}

.loading-bg{
  background: #fff !important;
  z-index:9999;
}