}
.cont_e_b{
  color: #9a9a9a;
  font-size: 15px;
  margin-left: 50px;
}
.cont_e_b icon{
  color: #0e9deb;
}
.cont_e_bottom{
  border-top: 1rpx solid #dddddd;
  margin-top: 15px;
  display: flex;
}
.cont_e_bottom .active,.cont_e_bottom .active .iconfont{
  color: #0a9ce9;
}
.cont_e_b .icon-tishi{
  color:red;
}
.cont_e_bottom_e .iconfont{
  font-size: 22px;
  color: #9a9a9a;
}
.cont_e_bottom_e{
  text-align: center;
  flex: 1;
  font-size: 15px;
  border-right: 1rpx solid #dddddd;
  margin-top: 10px;
  margin-bottom: 10px;
  color: #9a9a9a;
}
.border0{
  border: 0px;
}
.newBuiltBtn{
  width: 70px;
  height: 70px;
  position: fixed;
  bottom: 5px;
  right: 5px;
  z-index: 9999;
}

.main{
    width: 100%;
    height: auto;
    background:#f2f2f2;
}
.res-empty{
  width:100%;
  height: 50px;
  text-align:center;
  line-height:50px;
  color: #333;
  background: #f2f2f2;
}
.loadEffect{
  width: 30px;
  height: 50px;
  position: relative;
  margin: 0 auto;
}
.grey-icon{
  color:#9a9a9a !important; 
}
.span{
    display: inline-block;
    width: 5px;
    height: 5px;
    border-radius: 50%;
    background: #333;
    position: absolute;
    animation: load 1.04s ease infinite;
    -webkit-animation: load 1.04s ease infinite;
    -o-animation: load 1.04s ease infinite;
    -moz-animation: load 1.04s ease infinite;
}
@keyframes load{
    0%{
        opacity: 1;
    }
    100%{
        opacity: 0.2;
    }
}
.span:nth-child(1){
    left: 0;
    top: 50%;
    animation-delay:0.13s;
}
.span:nth-child(2){
    left: 40%;
    top: 50%;
    animation-delay:0.26s;
}
.span:nth-child(3){
    right: 0;
    top: 50%;
    animation-delay:0.39s;
}
