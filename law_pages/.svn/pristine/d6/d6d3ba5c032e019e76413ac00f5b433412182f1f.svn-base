<div style="padding-top: 10px; padding-bottom: 62px;">
    <div class="row serial_number_box line-height2-0">
        <div class="col-xs-12">
            <h4 class="font-weight-bold">流水号：<span>{{appraisalInfo.serialNo}}</span></h4>
        </div>
        <div class="second-color">
          <div class="col-xs-4">委托主体：<span>{{appraisalInfo.principalAgent | id2Text:principalAgentList}}</span></div>
          <div class="col-xs-4">鉴定用途：<span>{{appraisalInfo.identificationPurpose | id2Text:identificationPurposeList}}</span></div>
          <div class="col-xs-4">委托单位：<span>{{appraisalInfo.createOrgName | orgNull}}</span></div>
          <div class="col-xs-4">申请日期：<span>{{appraisalInfo.appalyDate | stringDate|date: 'yyyy-MM-dd'}}</span></div>
          <div class="col-xs-12 serial_number_box_cont">
            <div class="col-xs-6">鉴定机构：<span>{{appraisalInfo.appraisalOrgName}}</span></div>
            <div class="col-xs-6">许可证号：<span>{{appraisalInfo.extPro.licence}}</span></div>
            <div class="col-xs-6">法人：<span>{{appraisalInfo.extPro.legalPerson}}</span></div>
            <div class="col-xs-6">负责人：<span>{{appraisalInfo.extPro.personLiable}}</span></div>
            <div class="col-xs-6">联系电话：<span>{{appraisalInfo.extPro.telephone}}</span></div>
            <div class="col-xs-6">传真：<span>{{appraisalInfo.extPro.fax}}</span></div>
            <div class="col-xs-6">邮箱：<span>{{appraisalInfo.extPro.email}}</span></div>
            <div class="col-xs-12">鉴定类型：<span>{{appraisalInfo.extPro.appraisalItem | id2AppraisalItem:appraisalTypeList}}</span></div>
            <div class="col-xs-12">业务范围：<span>{{appraisalInfo.extPro.range}}</span></div>
            <div class="col-xs-12">机构地址：<span>{{appraisalInfo.extPro.address}}</span></div>
          </div>
        </div>
    </div>
    <div class="row identificationOfPeople" >
        <h4 class="col-xs-12">
            <span class="pull-left font-weight-bold">被鉴定人：</span>
            <span class="pull-left font-weight-bold people_information_icon_box" style="position: relative;">{{appraisalDetail.jyAppraisalApplyerInfo.personName}}
                <i class="iconfont icon-jibenxinxi2 margin-left-5 margin-bottom-0 " style="color: #264cb8; cursor: pointer;"></i>
                <div class="people_information">
                    <i class="iconfont icon-sanjiaoxian"></i>
                    <p>姓名：<span>{{appraisalDetail.jyAppraisalApplyerInfo.personName}}</span></p>
                    <p>姓别：<span>{{appraisalDetail.jyAppraisalApplyerInfo.sex | id2Text:sexList}}</span></p>
                    <p>证件类型：<span>{{appraisalDetail.jyAppraisalApplyerInfo.certificatesType | id2Text:certificateTypeList}}</span></p>
                    <p>证件号码：<span>{{appraisalDetail.jyAppraisalApplyerInfo.idNo}}</span></p>
                    <p>出生日期：<span>{{appraisalDetail.jyAppraisalApplyerInfo.birthDay | stringDate|date: 'yyyy-MM-dd'}}</span></p>
                    <p>手机号码：<span>{{appraisalDetail.jyAppraisalApplyerInfo.telephone}}</span></p>
                    <p>居住地：<span>{{appraisalDetail.jyAppraisalApplyerInfo.residence}}</span></p>
                </div>
            </span>
        </h4>
      <div class="col-xs-4 line-height-30">申请人：<span>{{appraisalDetail.applyName}}</span></div>
      <div class="col-xs-4 line-height-30">鉴定申请号：<span>{{appraisalDetail.appraisalNo}}</span></div>
      <div class="col-xs-4 line-height-30">鉴定类型：<span>{{appraisalDetail.appraisalType | id2Text:appraisalTypeList}}</span></div>
      <div class="col-xs-12 line-height-30">鉴定项目：<span>{{appraisalDetail.appraisalItem | id2AppraisalItem:appraisalItemList}}</span></div>
      <div class="col-xs-12 border-bottom mar-top-10 margin-bottom-10"></div>
      <div class="col-xs-12 brand-color">
        <span class="cur-pointer" ng-click="queryEvidence(appraisalDetail)"><i class="iconfont icon-zheng"></i> 查看证据</span>
        <span class="cur-pointer" ng-click="queryPersonDetail()"><i class="iconfont icon-staff-copy margin-left-30"></i> 当事人信息</span>
      </div>
    </div>
    <div class="row">
      <div class="module-box bg-default">
        <div class="nav-line-left">
          <h4>
            <span class="h1_line"></span>鉴定处理信息
            <span class=" pull-right ft12">
                <a class="brand-color cursor_pointer no-underline" ng-click="downloadSignFile()" ng-show="appraisalDetail.downloadSignShow"><i class="iconfont icon-xiazai"></i><span>下载签字页</span></a>
                <span class="brand-color cursor_pointer mar-le-10"   ngf-pattern="'image/*'" ngf-select="uploadSignFile($files)" ng-click="checkflash()" ngf-multiple="true"><i class="iconfont icon-shangchuan"></i><span>上传签字页</span></span>
                <a class="brand-color cursor_pointer no-underline mar-le-10" ng-href="{{downloadReportFile()}}" ng-show="appraisalDetail.downloadReportShow "><i class="iconfont icon-xiazai"></i><span>下载鉴定报告</span></a>
                <span class="brand-color cursor_pointer mar-le-10" accept="application/msword, application/pdf" ngf-pattern=".doc,.docx,.pdf" ng-click="checkflash()" ngf-select="uploadReportFile($file)" ><i class="iconfont icon-shangchuan"></i><span>上传鉴定报告</span></span>
              </span>
          </h4>
        </div>
        <div class="module-content">
          <form role="form" class="form-horizontal form-input">
            <div class="row clearfix ">
              <div class="form-group col-xs-6">
                <div class="col-lg-3 col-xs-4 text-right">
                  <label class=" control-label"><i>*</i> 案号</label>
                </div>
                <div class="col-lg-9 col-xs-8" ng-class="{'has-error': appraisalDetail.lawNoError }">
                  <input type="text" class="form-control"  placeholder="请输入案号" ng-model="appraisalDetail.lawNo" ng-blur="deal()">
                </div>
              </div>
              <div class="form-group col-xs-6">
                <div class="col-lg-3 col-xs-4 text-right">
                  <label class=" control-label"><i>*</i> 鉴定日期</label>
                </div>
                <div class="col-lg-9 col-xs-8">
                  <div class="input-group" ng-class="{'has-error': appraisalDetail.appraisalDateError }">
                    <input type="text"  readonly class="form-control" datepicker-popup="yyyy-MM-dd" ng-model="appraisalDetail.appraisalDate" is-open="appraisalDetail.opened"   datepicker-options="{startingDay:1}"  show-weeks="false" show-button-bar="false" />
                    <span class="input-group-btn">
                    <button type="button" class="btn btn-default" ng-click="open($event,appraisalDetail)"><i class="glyphicon glyphicon-calendar"></i></button>
                  </span>
                  </div>
                </div>
              </div>
            </div>
            <div class="row clearfix ">
              <div class="form-group col-xs-6">
                <div class="col-lg-3 col-xs-4 text-right">
                  <label class="control-label"><i>*</i> 鉴定地点</label>
                </div>
                <div class="col-lg-9 col-xs-8" ng-class="{'has-error': appraisalDetail.appraisalPlaceError }">
                  <input type="text" class="form-control"  placeholder="请输入鉴定地点" ng-model="appraisalDetail.appraisalPlace">
                </div>
              </div>
              <div class="form-group col-xs-6">
                <div class="col-lg-3 col-xs-4 text-right">
                  <label class=" control-label"><i>*</i> 主鉴定人</label>
                </div>
                <div class="col-lg-9 col-xs-8" ng-class="{'has-error': appraisalDetail.chiefAppraiserIdError }">
                  <select class="form-control input-sm" ng-model="appraisalDetail.chiefAppraiserId" ng-change="changeAppraiser(appraisalDetail.chiefAppraiserId,appraisalDetail)" ng-options="x.id as x.text  for x in chiefAppraiserList">
                    <option value="">请选择</option>
                  </select>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
      <div class="module-box bg-default" style="padding-bottom: 20px;">
        <div class="nav-line-left">
          <h4><span class="h1_line"></span>处理意见</h4>
        </div>
        <div style="padding: 20px;">
          <div class="col-xs-2 text-center">
            <label class=" control-label radio-label"> <i class="Required">*</i> 审批意见</label>
          </div>
          <div class="col-xs-6" >
            <label class="radio-inline col-xs-3" >
              <input type="radio" name="state" ng-value="0" ng-model="workFlow.result">同意处理
            </label>
            <label class="radio-inline col-xs-3" >
              <input type="radio" name="state" ng-value="2" ng-model="workFlow.result" >补充证据
            </label>
            <label class="radio-inline col-xs-3" >
              <input type="radio" name="state" ng-value="1" ng-model="workFlow.result" >退回
            </label>
          </div>
        </div>
        <textarea class="opinion_textarea" ng-show="workFlow.result == 0" ng-class="{'has-error': workFlow.remarkError }" placeholder="请输入处理意见（非必填）" ng-model="workFlow.remark"></textarea>
        <textarea class="opinion_textarea" ng-show="workFlow.result != 0" ng-class="{'has-error': workFlow.remarkError}" placeholder="请输入处理意见（必填）" ng-model="workFlow.remark"></textarea>
        <!--历史记录-->
        <div history-record ></div>
      </div>
    </div>

</div>
<!-- 底部按钮 -->
<div class="appraisal_footer" >
    <button type="button" class="btn btn-brand pull-right" ng-click="submitAppraisalDetail()" ng-disabled="dealLawNo">提交</button>
    <button type="button" class="btn btn-default pull-right" onclick="javascript:history.go(-1)">返回</button>
</div>