<div class="role_management_d relative_top">
  <div class="row bg-default">
    <div class="col-xs-12 role_management_right_d">
      <div class="role_management_right_t_d">
        <div class="col-xs-7 role_management_right_t_l_d no-padding">居民收入支出标准</div>
        <div class="col-lg-4 col-xs-4 line-height40 margin-top-6">
          <div class="search_box">
            <input type="text" class="input-xs search_input" ng-model="mtDenizenIncomeNorm.regionName" placeholder="区域名称">
            <i class="iconfont icon-sousuo input-search-img"></i>
          </div>
        </div>
        <div class="col-lg-1 col-xs-1 line-height40 no-padding margin-top-6">
          <button type="button" class="btn btn-brand mar-v-3"  ng-click="getDataInfo()" >搜索</button>
        </div>
      </div>
      <div class="clear"></div>
      <div class="panel-body">
        <div class="table-responsive">
          <div class="role_management_right_t_r_d" ng-click="newlyAdded()"><i class="iconfont icon-tianjia"></i>  新增</div>
          <table class="table table-hover" style="text-align: center;">
            <thead>
            <tr>
              <th class="col-xs-2" style="text-align: center;">区域名称</th>
              <th class="col-xs-1" style="text-align: center;">城镇死亡/伤残</th>
              <th class="col-xs-1" style="text-align: center;">农村死亡/伤残</th>
              <th class="col-xs-2" style="text-align: center;">城镇被抚养人生活费</th>
              <th class="col-xs-2" style="text-align: center;">农村被抚养人生活费</th>
              <th class="col-xs-1" style="text-align: center;">起始时间</th>
              <th class="col-xs-1" style="text-align: center;">终止时间</th>
              <th class="col-xs-1" style="text-align: center;">丧葬费标准</th>
              <th class="col-xs-1" style="text-align: center;">操作</th>
            </tr>
            </thead>
            <tbody>
            <tr ng-repeat="norm in pageData.showList">
                <td style="position: relative;" style="line-height: 35px;">
                    {{norm.regionName}}
                </td>
                <td style="line-height: 35px;">
                    {{norm.urbanDisposableIncome}}
                </td>

                <td style="line-height: 35px;">
                   {{norm.ruralNetIncome}}
                </td>

                <td style="line-height: 35px;">
                    {{norm.urbanAverageOutlay}}
                </td>

                <td style="line-height: 35px;">
                    {{norm.ruralAverageOutlay}}
                </td>

                <td style="line-height: 35px;">
                    {{norm.startDate|limitTo:10}}
                </td>

                <td style="line-height: 35px;">
                    {{norm.endDate|limitTo:10}}
                </td>

                <td style="line-height: 35px;">
                    {{norm.funeralFeeStandard}}
                </td>

                <td class="action_bar" style="line-height: 35px;">
                    <i class="iconfont icon-iconfont19" style="font-size:18px;" ng-click="deleteNorm(norm)" title="删除"></i>
                    <i class="iconfont icon-bianji" style="font-size:18px;"  ng-click="newlyAdded(norm)" title="修改"></i>
                </td>
            </tr>
            </tbody>
          </table>
            <div class="text-center"  ng-if="pageData.showList.length != 0 ">
                <pagination previous-text="<" next-text=">" first-text="<<" last-text=">>" items-per-page="pageData.itemNum" class="pagination-custom" total-items="pageData.totalItems" ng-model="pageData.currentPage" ng-change="pageChanged()" max-size="7" boundary-links="true" rotate="false"></pagination>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>