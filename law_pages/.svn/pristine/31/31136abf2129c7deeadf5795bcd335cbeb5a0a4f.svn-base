<div style="padding-bottom: 30px;">
    <div class="bg-default ng-scope row" ng-if="co.tabFlag == 'case'">
        <div style="border: 1px solid #ddd;" class="bg-default module-box" style="padding-bottom: 10px;">
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <i class="caseDetailIcon icon1"></i>
                    事故信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">事故编号：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.accidentCode}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">事故时间：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.accidentTime | stringDate | date:'yyyy-MM-dd'}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">事故地点：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.accidentPlace}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">是否高速：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.highSpeed == '1'? '是':'否'}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">死亡人数：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.deathNumber}}人</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">受伤人数：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.injuredNumber}}人</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">事故类型：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.accidentType | id2Text : accidentTypeList}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">现场形态：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.scene | id2Text : sceneList}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-2 text-right text-muted">简要案情：</div>
                                    <div class="col-xs-10 no-padding ng-binding">{{police.accidentInfo.briefCase}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">直接财产损失：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.propertyLoss}}元</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">事故认定原因：</div>
                                    <div class="col-xs-1 no-padding ng-binding">{{police.accidentInfo.accidentCauseCode}}</div>
                                    <div class="col-xs-7 no-padding ng-binding">{{police.accidentInfo.accidentCauseRemark}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">现场勘查日期：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.surveyDate | stringDate | date:'yyyy-MM-dd'}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">是否检验鉴定：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{police.accidentInfo.isAppraisal == '1'? '是': '否'}}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div class="bg-default ng-scope row" ng-if="co.tabFlag == 'parties'" ng-repeat="info in police.jpoliceVehicVO">
        <div style="border: 1px solid #ddd;" class="bg-default module-box" style="padding-bottom: 10px;">
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <i class="caseDetailIcon icon4"></i>
                    人员信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="col-xs-6 form-horizontal ng-pristine ng-invalid ng-invalid-required">
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">姓名：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.userName}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">性别：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.sex == '1'? '男':'女'}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">身份证号：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.personCard}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">年龄：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.age}}</div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-6" style="margin-top: -10px;">
                            <div class="col-lg-6" ng-class="{'z-index-img':info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1}">
                                <div class="margin_top img_box_boder Front img_width">
                                    <img style="height: 177px;cursor: pointer;" class="id_img" ng-src="{{info.policePersonInformation.identityCardApic && info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?imageAddress+info.policePersonInformation.identityCardApic:co.defaultImg2}}" ng-click="zoomImage(info.policePersonInformation.identityCardApic && info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?bigPictureUrl+info.policePersonInformation.identityCardApic:co.defaultImg2)">
                                    <div class="img_bottom_bar">
                                        <p><span>证件照身份证号面</span></p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6"  ng-class="{'z-index-img':police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1}">
                                <div class=" margin_top img_box_boder Back img_width">
                                    <img style="height: 177px;cursor: pointer;" class="id_img" ng-src="{{info.policePersonInformation.identityCardBpic && info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?imageAddress+info.policePersonInformation.identityCardBpic:co.defaultImg3}}" ng-click="zoomImage(info.policePersonInformation.identityCardBpic && info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?bigPictureUrl+info.policePersonInformation.identityCardBpic:co.defaultImg3)">
                                    <div class="img_bottom_bar">
                                        <p><span>证件照国徽面</span></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-2 text-right text-muted">户籍地：</div>
                                <div class="col-xs-10 no-padding ng-binding" style="word-break: break-all">{{info.policePersonInformation.placeOfDomicile}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-2 text-right text-muted">实际居住地：</div>
                                <div class="col-xs-10 no-padding ng-binding">{{info.policePersonInformation.address}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-2 text-right text-muted">交通方式：</div>
                                <div class="col-xs-10 no-padding ng-binding">{{info.policePersonInformation.trafficType | id2Text : trafficTypeList}}</div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!--就医信息-->
        <div style="border: 1px solid #ddd;" class="module-box bg-default" style="padding-bottom: 10px;">
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <i class="caseDetailIcon icon5"></i>
                    机动车及保单信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div style="border-bottom: 1px dashed #ddd" ng-repeat="vehicle in info.vehiclePolicyInfoList">
                        <div class="row clearfix ">
                            <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                                <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                    <div class="col-xs-12 margin_top">
                                        <div class="col-xs-4 text-right text-muted">车辆类型：</div>
                                        <div class="col-xs-8 no-padding ng-binding">{{vehicle.vehiclePolicyInfo.vehicleType | id2Text : vehicleTypeList}}</div>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                    <div class="col-xs-12 margin_top">
                                        <div class="col-xs-4 text-right text-muted">号牌种类：</div>
                                        <div class="col-xs-8 no-padding ng-binding">{{vehicle.vehiclePolicyInfo.plateType | id2Text : plateTypeList}}</div>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                    <div class="col-xs-12 margin_top">
                                        <div class="col-xs-4 text-right text-muted">号牌号码：</div>
                                        <div class="col-xs-8 no-padding ng-binding">{{vehicle.vehiclePolicyInfo.palteNo}}</div>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                    <div class="col-xs-12 margin_top">
                                        <div class="col-xs-4 text-right text-muted">车辆损失：</div>
                                        <div class="col-xs-8 no-padding ng-binding">{{vehicle.vehiclePolicyInfo.vehicleLoss | id2Text : vehicleLossList}}</div>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                                    <div class="col-xs-12 margin_top">
                                        <div class="col-xs-4 text-right text-muted">投保险种：</div>
                                        <div class="col-xs-8 no-padding ng-binding">{{riskTypesCheckedFun(vehicle.vehiclePolicyInfo.riskTypes, riskTypesList)}}</div>
                                    </div>
                                </div>
                                <div class="col-xs-11 form-group form-group-sm" style="margin-top: 15px;">
                                    <table class="table table-bordered col-xs-offset-1 tab2-table">
                                        <thead>
                                        <tr>
                                            <th colspan="2">险种名称</th>
                                            <th>保额</th>
                                            <th>报案号</th>
                                            <th width="30%">承保公司</th>
                                            <th>免赔率%</th>
                                            <th>赔偿比例%</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-if="vehicle.vehicleCompany[0].riskTypes == '0'">
                                                <td rowspan="3">交强险</td>
                                                <td>医疗费</td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].medicalFees" ng-bind="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].medicalFees=info.policePersonInformation.accidentLiability == '4'? '1000' : '10000'" disabled></td>
                                                <td rowspan="3"><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].reportNo" ng-disabled="vehicle.vehicleCompany[0].sendState == '3'"></td>
                                                <td rowspan="3">
                                                    <div class="col-xs-12" style="position: relative;">
                                                        <input type="text" class="form-control" ng-focus="insuranceFocus('insuranceForceCompany')" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].insuranceCompany" ng-blur="blurInsurance('insuranceForceCompany')" placeholder="请填写承保保险公司" readonly maxlength="50" disabled>
                                                        <ul class="select_border position_ul" ng-show="insuranceShow1">
                                                            <li ng-repeat="x in insuranceList" ng-click="selectItems(vehicle, x, 'vehicleCompany', '0')">{{x.text}}</li>
                                                        </ul>
                                                    </div>
                                                </td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr ng-if="vehicle.vehicleCompany[0].riskTypes == '0'">
                                                <td>死亡伤残</td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].disability" ng-bind="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].disability=info.policePersonInformation.accidentLiability == '4'? '11000' : '110000'" disabled></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr ng-if="vehicle.vehicleCompany[0].riskTypes == '0'">
                                                <td>财产损失</td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].propertyLoss" ng-bind="(vehicle.vehicleCompany | filter:{riskTypes: '0'})[0].propertyLoss=info.policePersonInformation.accidentLiability == '4'? '100' : '2000'" disabled></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr ng-if="vehicle.vehicleCompany[0].riskTypes == '1'">
                                                <td>商业险</td>
                                                <td>三者险</td>
                                                <td><input ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '1'})[0].thirdParty" type="text" float-only ng-disabled="vehicle.vehicleCompany[0].sendState == '3'"></td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '1'})[0].reportNo" ng-disabled="vehicle.vehicleCompany[0].sendState == '3'"></td>
                                                <td>
                                                    <div class="col-xs-12" style="position: relative;">
                                                        <input type="text" class="form-control" ng-focus="insuranceFocus('insuranceBusinessCompany')" ng-blur="blurInsurance('insuranceBusinessCompany')" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '1'})[0].insuranceCompany" placeholder="请填写承保保险公司" readonly maxlength="50" disabled>
                                                        <ul class="select_border position_ul" ng-show="insuranceShow2">
                                                            <li ng-repeat="x in insuranceList" ng-click="selectItems(vehicle, x, 'vehicleCompany', '1')">{{x.text}}</li>
                                                        </ul>
                                                    </div>
                                                </td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '1'})[0].thirdPartyFranchise" ng-disabled="vehicle.vehiclePolicyInfo.riskTypes.indexOf('2') != -1 || vehicle.vehicleCompany[0].sendState == '3'" ng-change="inputVerify((vehicle.vehicleCompany | filter:{riskTypes: '1'})[0], 'thirdPartyFranchise', 100, 'thirdPartyRate')"></td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '1'})[0].thirdPartyRate" ng-disabled="vehicle.vehiclePolicyInfo.riskTypes.indexOf('2') != -1 || vehicle.vehicleCompany[0].sendState == '3'" ng-change="inputVerify((vehicle.vehicleCompany | filter:{riskTypes: '1'})[0], 'thirdPartyRate', 100, 'thirdPartyFranchise')"></td>
                                            </tr>
                                            <tr ng-if="vehicle.vehicleCompany[0].riskTypes == '2'">
                                                <td>商业险</td>
                                                <td>车损险</td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '2'})[0].vehicleLossInsurance" float-only ng-disabled="vehicle.vehicleCompany[0].sendState == '3'"></td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '2'})[0].reportNo" ng-disabled="vehicle.vehicleCompany[0].sendState == '3'"></td>
                                                <td>
                                                    <div class="col-xs-12" style="position: relative;">
                                                        <input type="text" class="form-control" ng-focus="insuranceFocus('insuranceVehicleCompany')" ng-blur="blurInsurance('insuranceVehicleCompany')" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '2'})[0].insuranceCompany" placeholder="请填写承保保险公司" readonly maxlength="50" disabled>
                                                        <ul class="select_border position_ul" ng-show="insuranceShow3">
                                                            <li ng-repeat="x in insuranceList" id="{{x.id}}" ng-click="selectItems(vehicle, x, 'vehicleCompany', '2')">{{x.text}}</li>
                                                        </ul>
                                                    </div>
                                                </td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '2'})[0].vehicleRate" ng-disabled="vehicle.vehicleCompany[0].sendState == '3'" ng-change="inputVerify((vehicle.vehicleCompany | filter:{riskTypes: '2'})[0], 'vehicleRate', 100, 'vehiceFranchise')"></td>
                                                <td><input type="text" ng-model="(vehicle.vehicleCompany | filter:{riskTypes: '2'})[0].vehiceFranchise" ng-disabled="vehicle.vehicleCompany[0].sendState == '3'" ng-change="inputVerify((vehicle.vehicleCompany | filter:{riskTypes: '2'})[0], 'vehiceFranchise', 100, 'vehicleRate')"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div id="big_img_box">
    <div class="big_img_box_bj"></div>
    <div class="big_img_box_img_box">
        <div id="big_img_box_img_f" style="position: absolute;">
            <div id="big_img_box_img_bar" style="width: 100%; height: 100%;" title="可以拖拽移动"></div>
            <img id="big_img_box_img" src="imgSrc">
        </div>
        <div class="big_img_box_bottom">
            <div class="big_img_box_bottom_bg"></div>
            <i class="iconfont icon-qiepian30" onclick="rotateImg();"></i>
            <i class="iconfont icon-iconfontfangda" onclick="enlargeimg();"></i>
            <i class="iconfont icon-iconfontsuoxiao" onclick="narrowimg();"></i>
        </div>
        <i class="iconfont icon--tingqiantiaojieduihao-Xhao" onclick="hideImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy-copy" onclick="leftImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy" onclick="rightImg();"></i>
    </div>
</div>
<script type="text/javascript">
  var oBox = document.getElementById("big_img_box_img_f");
  var oBar = document.getElementById("big_img_box_img_bar");
  startDrag(oBar, oBox);
</script>
