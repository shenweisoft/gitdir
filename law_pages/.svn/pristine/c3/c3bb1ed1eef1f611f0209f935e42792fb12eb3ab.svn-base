
<div class="bg-default ng-scope row">
    <div style="border: 1px solid #ddd;" class="bg-default module-box padding-bottom: 10px;">
        <div class="nav-line-left">
            <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                <i class="caseDetailIcon icon6"></i>
                伤者信息
            </h4>
        </div>
        <div class="module-content font-size13">
            <form role="form" class="form-horizontal form-input">
                <!--新增-->
                <div class="row clearfix" ng-if="co.stateParams.flag == 'healthInfoFill'">
                    <div class="col-xs-6 form-horizontal ng-pristine ng-invalid ng-invalid-required">
                        <div class="form-group col-xs-12">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class="control-label"><i>*</i> 姓名</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.hospitalPersonInfo.userNameError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.hospitalPersonInfo.userName" placeholder="请输入姓名">
                            </div>
                        </div>
                        <div class="col-xs-12 form-group form-group-sm">
                            <label class="col-xs-4 control-label">
                                <span class="Required">*</span>
                                <span>性别</span>
                            </label>
                            <div class="col-xs-8">
                                <div class="radio" style="padding-top: 0px;" ng-class="{'has-error': police.hospitalPersonInfo.sexError}">
                                    <label>
                                        <input type="radio" ng-model="police.hospitalPersonInfo.sex" value="1" ng-checked="police.hospitalPersonInfo.userName == '1'">男
                                    </label>
                                    <label>
                                        <input type="radio" ng-model="police.hospitalPersonInfo.sex" value="0" ng-checked="police.hospitalPersonInfo.userName == '0'">女
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label"><i>*</i> 身份证号</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.hospitalPersonInfo.personCardError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.hospitalPersonInfo.personCard" ng-blur="checkIdentity(police.hospitalPersonInfo, false)" ng-change="changePersonCard(police.hospitalPersonInfo)" placeholder="请输入身份证号">
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label"><i>*</i> 年龄</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.hospitalPersonInfo.ageError}">
                                <input type="text" maxlength="3" class="form-control ng-pristine ng-valid" ng-model="police.hospitalPersonInfo.age" placeholder="请输入年龄" int-only>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-6" style="margin-top: -10px;">
                        <div class="col-lg-6" ng-class="{'z-index-img':police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg)==-1}">
                            <div class="margin_top img_box_boder Front img_width">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.hospitalPersonInfo.identityCardApic && police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg2)==-1 ?imageAddress+police.hospitalPersonInfo.identityCardApic:co.defaultImg2}}" ngf-pattern="'image/*'" accept="image/gif,image/jpeg,image/jpg,image/png,image/svg" ngf-select="addEvidence($file, police.hospitalPersonInfo, 'face')" ng-click="checkflash()">
                                <div class="img_bottom_bar">
                                    <p><span>证件照身份证号面</span><i class="iconfont icon-iconfont19" ng-click="removeApplicantImage(police.hospitalPersonInfo, 'A', 'identityCardApic')" ng-show="police.hospitalPersonInfo.identityCardApic"></i></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6"  ng-class="{'z-index-img':police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg)==-1}">
                            <div class=" margin_top img_box_boder Back img_width">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.hospitalPersonInfo.identityCardBpic && police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg3)==-1 ?imageAddress+police.hospitalPersonInfo.identityCardBpic:co.defaultImg3}}" ngf-pattern="'image/*'" accept="image/gif,image/jpeg,image/jpg,image/png,image/svg" ngf-select="addEvidence($file, police.hospitalPersonInfo, 'back')" ng-click="checkflash()">
                                <div class="img_bottom_bar">
                                    <p><span>证件照国徽面</span><i class="iconfont icon-iconfont19"  ng-click="removeApplicantImage(police.hospitalPersonInfo, 'B', 'identityCardBpic')" ng-show="police.hospitalPersonInfo.identityCardBpic"></i></p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="col-lg-2 col-xs-2 text-right">
                            <label class=" control-label"><i>*</i> 户籍地</label>
                        </div>
                        <div class="col-lg-10 col-xs-10" ng-class="{'has-error': police.hospitalPersonInfo.placeDomicileError}">
                            <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.hospitalPersonInfo.placeDomicile" placeholder="请输入户籍地">
                        </div>
                    </div>
                    <div class="form-group col-xs-12">
                        <div class="col-lg-2 col-xs-2 text-right">
                            <label class=" control-label"><i>*</i> 实际居住地</label>
                        </div>
                        <div class="col-lg-10 col-xs-10" ng-class="{'has-error': police.hospitalPersonInfo.addressError}">
                            <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.hospitalPersonInfo.address" placeholder="请输入实际居住地">
                        </div>
                    </div>
                </div>

                <!--补录-->
                <div class="row clearfix" ng-if="co.stateParams.flag == 'healthInfoSupplement' || co.stateParams.tableName">
                    <div class="col-xs-6 form-horizontal ng-pristine ng-invalid ng-invalid-required">
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">姓名：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{stateParams.tableName? police.hospitalPersonInfo.userName : police.policePersonInformation.userName}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">性别：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{stateParams.tableName? (police.hospitalPersonInfo.sex == '1'? '男': '女') : (police.policePersonInformation.sex == '1'? '男': '女')}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">身份证号：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{stateParams.tableName? police.hospitalPersonInfo.personCard : police.policePersonInformation.personCard}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">年龄：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{stateParams.tableName? police.hospitalPersonInfo.age : police.policePersonInformation.age}}</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-6" style="margin-top: -10px;" ng-if="stateParams.tableName">
                        <div class="col-lg-5" ng-class="{'z-index-img':police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg)==-1}">
                            <div class="margin_top img_box_boder Front img_width">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.hospitalPersonInfo.identityCardApic && police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg2)==-1 ?imageAddress+police.hospitalPersonInfo.identityCardApic:co.defaultImg2}}" ng-click="zoomImage(police.hospitalPersonInfo.identityCardApic && police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardApic.indexOf(co.defaultImg2)==-1 ?bigPictureUrl+police.hospitalPersonInfo.identityCardApic:co.defaultImg2)">
                                <div class="img_bottom_bar">
                                    <p><span>证件照身份证号面</span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-5"  ng-class="{'z-index-img':police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg)==-1}">
                            <div class=" margin_top img_box_boder Back img_width">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.hospitalPersonInfo.identityCardBpic && police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg3)==-1 ?imageAddress+police.hospitalPersonInfo.identityCardBpic:co.defaultImg3}}" ng-click="zoomImage(police.hospitalPersonInfo.identityCardBpic && police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg)==-1&&police.hospitalPersonInfo.identityCardBpic.indexOf(co.defaultImg3)==-1 ?bigPictureUrl+police.hospitalPersonInfo.identityCardBpic:co.defaultImg3)">
                                <div class="img_bottom_bar">
                                    <p><span>证件照国徽面</span></p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-6" style="margin-top: -10px;" ng-if="!stateParams.tableName">
                        <div class="col-lg-6" ng-class="{'z-index-img':police.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1}">
                            <div class="margin_top img_box_boder Front img_width" style="width:247px">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.policePersonInformation.identityCardApic && police.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&police.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?imageAddress+police.policePersonInformation.identityCardApic:co.defaultImg2}}" ng-click="zoomImage(police.policePersonInformation.identityCardApic && police.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&police.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?bigPictureUrl+police.policePersonInformation.identityCardApic:co.defaultImg2)">
                                <div class="img_bottom_bar">
                                    <p><span>证件照身份证号面</span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6"  ng-class="{'z-index-img':police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1}">
                            <div class=" margin_top img_box_boder Back img_width" style="width:247px">
                                <img style="height: 177px;" class="id_img" ng-src="{{police.policePersonInformation.identityCardBpic && police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?imageAddress+police.policePersonInformation.identityCardBpic:co.defaultImg3}}" ng-click="zoomImage(police.policePersonInformation.identityCardBpic && police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?bigPictureUrl+police.policePersonInformation.identityCardBpic:co.defaultImg3)">
                                <div class="img_bottom_bar">
                                    <p><span>证件照国徽面</span></p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                        <div class="col-xs-12 margin_top">
                            <div class="col-xs-2 text-right text-muted">户籍地：</div>
                            <div class="col-xs-10 no-padding ng-binding">{{stateParams.tableName? police.hospitalPersonInfo.placeDomicile : police.policePersonInformation.placeOfDomicile}}</div>
                        </div>
                    </div>
                    <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                        <div class="col-xs-12 margin_top">
                            <div class="col-xs-2 text-right text-muted">实际居住地：</div>
                            <div class="col-xs-10 no-padding ng-binding">{{stateParams.tableName? police.hospitalPersonInfo.address : police.policePersonInformation.address}}</div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <!--就医信息-->
    <div style="border: 1px solid #ddd;" class="module-box bg-default" style="padding-bottom: 10px;">
        <div class="nav-line-left">
            <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                <i class="caseDetailIcon icon7"></i>
                就医信息
            </h4>
        </div>
        <div class="module-content font-size13">
            <form role="form" class="form-horizontal form-input">
                <div class="row clearfix ">
                    <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0" ng-if="!stateParams.tableName">
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">就医医院：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{co.stateParams.id? police.healthDepartmentInfo.hospital : userDepart.orgName}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label"><i>*</i> 科室</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.healthDepartmentInfo.departmentError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.healthDepartmentInfo.department" placeholder="请输入科室">
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label"><i>*</i> 病房号</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.healthDepartmentInfo.roomNumberError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.healthDepartmentInfo.roomNumber" placeholder="请输入病房号">
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label"><i>*</i> 主治医师</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.healthDepartmentInfo.attendingDoctorError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.healthDepartmentInfo.attendingDoctor" placeholder="请输入主治医师">
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-xs-12">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 是否住院</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.healthDepartmentInfo.hospitalizationError}">
                                    <label class="radio-inline">
                                        <input type="radio" ng-model="police.healthDepartmentInfo.hospitalization" value="1" ng-checked="police.healthDepartmentInfo.hospitalization == '1'">是
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" ng-model="police.healthDepartmentInfo.hospitalization" value="0" ng-checked="police.healthDepartmentInfo.hospitalization == '0'">否
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-xs-6 row clearfix">
                            <label class="col-lg-4 col-xs-5 text-right  control-label"><i ng-show="police.healthDepartmentInfo.hospitalization == '1'">*</i> 入院时间</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.healthDepartmentInfo.beHospitTimeError}">
                                <div class="input-group">
                                    <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="police.healthDepartmentInfo.beHospitTime" is-open="police.healthDepartmentInfo.beHospitTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-default" ng-click="openBeHospitTime($event, police.healthDepartmentInfo)"><i class="glyphicon glyphicon-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-xs-6 row clearfix">
                            <label class="col-lg-4 col-xs-5 text-right control-label">出院时间</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.healthDepartmentInfo.outHospitTimeError}">
                                <div class="input-group">
                                    <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="police.healthDepartmentInfo.outHospitTime" is-open="police.healthDepartmentInfo.outHospitTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-default" ng-click="openHealthDepartment($event, police.healthDepartmentInfo)"><i class="glyphicon glyphicon-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;" ng-if="co.stateParams.flag != 'healthInfoFill'">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">补充状态：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.sendType == '2'? '已补充':'待补充'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;" ng-if="co.stateParams.flag == 'healthInfoFill'">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">关联状态：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.relationState == '1'? '已关联':'待关联'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label">三日内医疗费</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.healthDepartmentInfo.thrMediFeError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.healthDepartmentInfo.thrMediFe" placeholder="请输入三日内医疗费">
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-4 text-right">
                                <label class=" control-label">后期医疗费</label>
                            </div>
                            <div class="col-lg-8 col-xs-8" ng-class="{'has-error': police.healthDepartmentInfo.laterMedicalFeeError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.healthDepartmentInfo.laterMedicalFee" placeholder="请输入后期医疗费">
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <div class="col-lg-2 col-xs-2 text-right">
                                <label class=" control-label">伤情诊断</label>
                            </div>
                            <div class="col-lg-1 col-xs-1" style="margin-right: 20px;">
                                <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ngf-multiple="true" ngf-pattern=".doc,.docx,image/*,.pdf"  accept="image/gif,image/jpeg,image/jpg,image/png,image/svg,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document,application/pdf" ngf-select="addEvidence($files, police.healthDepartmentInfo)"  ng-click="checkflash()" ng-model="files">
                                    <i class="iconfont icon-shangchuan brand-color" style="color: #fff;"></i>
                                    上传
                                </button>
                            </div>
                            <div class="col-lg-1 col-xs-1" style="font-size: 16px;border-left: 1px solid #E4E4E4;padding-left: 30px;min-width: 100px;" ng-if="police.healthDepartmentInfo.enclosureInfoList.length > 0">
                                <label class="checkbox-inline">
                                    <input type="checkbox" ng-checked="police.healthDepartmentInfo.isAllChecked" ng-click="handleAllChecked($event, police.healthDepartmentInfo.enclosureInfoList, police.healthDepartmentInfo)">全选
                                </label>
                            </div>
                            <div class="col-lg-1 col-xs-1" ng-if="police.healthDepartmentInfo.enclosureArr.length && police.healthDepartmentInfo.enclosureInfoList.length > 0">
                                <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ng-click="deleteEnclosure(police.healthDepartmentInfo)">
                                    删除
                                </button>
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <div class="col-lg-2 col-xs-2 text-right"></div>
                            <div class="col-lg-10 col-xs-10">
                                <ul class="evidences">
                                    <li ng-repeat="enclosureInfo in police.healthDepartmentInfo.enclosureInfoList" >
                                        <i class="{{enclosureInfo.isChecked? 'icon-active':'icon-normal'}}" ng-click="handleEnclosure(enclosureInfo, police.healthDepartmentInfo.enclosureInfoList, police.healthDepartmentInfo)"></i>
                                        <!--<img ng-src="{{enclosureInfo.fileType == '0'? imageAddress+enclosureInfo.path : 'views/images/police-evidence-default.png'}}" alt="">-->
                                        <img ng-show="enclosureInfo.fileType == '0'" ng-src="{{imageAddress+enclosureInfo.path}}" ng-click="zoomImage(bigPictureUrl+enclosureInfo.path)">
                                        <a ng-click="downloadFile(enclosureInfo)">
                                            <img ng-show="enclosureInfo.fileType == '1'" ng-src="{{'views/images/police-evidence-default1.png'}}" alt="">
                                            <img ng-show="enclosureInfo.fileType == '2'" ng-src="{{'views/images/police-evidence-default2.png'}}" alt="">
                                        </a>
                                        <p>{{enclosureInfo.oldname}}</p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <!--关联查看-->
                    <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0" ng-if="stateParams.tableName">
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">就医医院：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.hospital}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">科室：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.department}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">病房号：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.roomNumber}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">主治医生：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.attendingDoctor}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">是否住院：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.hospitalization == '1'? '是': '否'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">入院时间：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.beHospitTime | stringDate | date:'yyyy-MM-dd'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">出院时间：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.outHospitTime | stringDate | date:'yyyy-MM-dd'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">关联状态：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.relationState == '1'? '已关联' : '待关联'}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">三日内医疗费：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.thrMediFe}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-4 text-right text-muted">后期医疗费：</div>
                                <div class="col-xs-8 no-padding ng-binding">{{police.healthDepartmentInfo.laterMedicalFee}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <div class="col-xs-2 text-right text-muted">伤情诊断：</div>
                            <div class="col-lg-10 col-xs-10">
                                <ul class="evidences">
                                    <li ng-repeat="enclosureInfo in police.healthDepartmentInfo.enclosureInfoList">
                                        <!--<img ng-src="{{enclosureInfo.fileType == '0'? imageAddress+enclosureInfo.path : 'views/images/police-evidence-default.png'}}" alt="">-->
                                        <img ng-show="enclosureInfo.fileType == '0'" ng-src="{{imageAddress+enclosureInfo.path}}" ng-click="zoomImage(bigPictureUrl+enclosureInfo.path)">
                                        <a ng-click="downloadFile(enclosureInfo)">
                                            <img ng-show="enclosureInfo.fileType == '1'" ng-src="{{'views/images/police-evidence-default.png'}}" alt="">
                                        </a>
                                        <p>{{enclosureInfo.oldname}}</p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<div id="big_img_box">
    <div class="big_img_box_bj"></div>
    <div class="big_img_box_img_box">
        <div id="big_img_box_img_f" style="position: absolute;">
            <div id="big_img_box_img_bar" style="width: 100%; height: 100%;" title="可以拖拽移动"></div>
            <img id="big_img_box_img" src="imgSrc">
        </div>
        <div class="big_img_box_bottom">
            <div class="big_img_box_bottom_bg"></div>
            <i class="iconfont icon-qiepian30" onclick="rotateImg();"></i>
            <i class="iconfont icon-iconfontfangda" onclick="enlargeimg();"></i>
            <i class="iconfont icon-iconfontsuoxiao" onclick="narrowimg();"></i>
        </div>
        <i class="iconfont icon--tingqiantiaojieduihao-Xhao" onclick="hideImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy-copy" onclick="leftImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy" onclick="rightImg();"></i>
    </div>
</div>
<script type="text/javascript">
  var oBox = document.getElementById("big_img_box_img_f");
  var oBar = document.getElementById("big_img_box_img_bar");
  startDrag(oBar, oBox);
</script>

