<div ng-show="(police.accidentInfo.id || police.accidentInfo.id == '0')">
    <div ng-repeat="info in police.jpoliceVehicVO | isWounded">
        <div class="module-box bg-default" style="padding-bottom: 10px;">
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="col-xs-6 form-horizontal ng-pristine ng-invalid ng-invalid-required">
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">姓名：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.userName}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">性别：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.sex == '1'? '男': '女'}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">身份证号：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.personCard}}</div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                                <div class="col-xs-12 margin_top">
                                    <div class="col-xs-4 text-right text-muted">年龄：</div>
                                    <div class="col-xs-8 no-padding ng-binding">{{info.policePersonInformation.age}}</div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-6" style="margin-top: -10px;">
                            <div class="col-lg-6" ng-class="{'z-index-img':info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1}">
                                <div class="margin_top img_box_boder Front img_width">
                                    <img style="height: 177px;cursor: pointer;" class="id_img" ng-src="{{info.policePersonInformation.identityCardApic && info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?imageAddress+info.policePersonInformation.identityCardApic:co.defaultImg2}}" ng-click="zoomImage(info.policePersonInformation.identityCardApic && info.policePersonInformation.identityCardApic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardApic.indexOf(co.defaultImg2)==-1 ?bigPictureUrl+info.policePersonInformation.identityCardApic:co.defaultImg2)">
                                    <div class="img_bottom_bar">
                                        <p><span>证件照身份证号面</span></p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6"  ng-class="{'z-index-img':police.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1}">
                                <div class=" margin_top img_box_boder Back img_width">
                                    <img style="height: 177px;cursor: pointer;" class="id_img" ng-src="{{info.policePersonInformation.identityCardBpic && info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?imageAddress+info.policePersonInformation.identityCardBpic:co.defaultImg3}}" ng-click="zoomImage(info.policePersonInformation.identityCardBpic && info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg)==-1&&info.policePersonInformation.identityCardBpic.indexOf(co.defaultImg3)==-1 ?bigPictureUrl+info.policePersonInformation.identityCardBpic:co.defaultImg3)">
                                    <div class="img_bottom_bar">
                                        <p><span>证件照国徽面</span></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-2 text-right text-muted">户籍地：</div>
                                <div class="col-xs-10 no-padding ng-binding">{{info.policePersonInformation.placeOfDomicile}}</div>
                            </div>
                        </div>
                        <div class="form-group col-xs-12" style="padding-bottom: 1px;">
                            <div class="col-xs-12 margin_top">
                                <div class="col-xs-2 text-right text-muted">实际居住地：</div>
                                <div class="col-xs-10 no-padding ng-binding">{{info.policePersonInformation.address}}</div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!--就医信息-->
        <div style="border: 1px solid #ddd;" class="module-box bg-default" style="padding-bottom: 10px;">
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <span class="h1_line"></span>
                    就医信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <!--此处需要循环-->
                    <div style="border-bottom: 1px dashed #ddd">
                        <div class="row clearfix ">
                            <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                                <div class="form-group col-lg-6 col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label"><i>*</i> 就医医院</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.hospitalIdError}">
                                        <select class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.hospitalId" ng-change="getValueById(info.healthDepartmentInfo.hospitalId, info.healthDepartmentInfo, 'hospital', (appraisalMechanismList | filter:{category : '#06'}), 'relationOrgName')">
                                            <option ng-selected="info.healthDepartmentInfo.hospitalId == x.relationOrgId" ng-value="x.relationOrgId" ng-repeat="x in (appraisalMechanismList | filter:{category : '#06'})">{{x.relationOrgName}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">科室</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.departmentError}">
                                        <input type="text" maxlength="200" class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.department" placeholder="请输入科室">
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">病房号</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.roomNumberError}">
                                        <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.roomNumber" placeholder="请输入病房号">
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">主治医师</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.attendingDoctorError}">
                                        <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.attendingDoctor" placeholder="请输入主治医师">
                                    </div>
                                </div>
                                <div class="col-xs-6 form-group form-group-sm">
                                    <div class="col-xs-12">
                                        <div>
                                            <label class="col-lg-4 col-xs-5 text-right  control-label">是否住院</label>
                                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.healthDepartmentInfo.hospitalizationError}">
                                                <label class="radio-inline">
                                                    <input type="radio" ng-model="info.healthDepartmentInfo.hospitalization" value="1" ng-checked="info.healthDepartmentInfo.hospitalization == '1'">是
                                                </label>
                                                <label class="radio-inline">
                                                    <input type="radio" ng-model="info.healthDepartmentInfo.hospitalization" value="0" ng-checked="info.healthDepartmentInfo.hospitalization == '0'">否
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">入院时间</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.healthDepartmentInfo.beHospitTimeError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="info.healthDepartmentInfo.beHospitTime" is-open="info.healthDepartmentInfo.beHospitTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openBeHospitTime($event, info.healthDepartmentInfo)"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">出院时间</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.healthDepartmentInfo.outHospitTimeError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="info.healthDepartmentInfo.outHospitTime" is-open="info.healthDepartmentInfo.outHospitTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openOutHospitTime($event, info.healthDepartmentInfo)"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">补充状态</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.sendTypeError}">
                                        <span style="display: none;" ng-bind="info.healthDepartmentInfo.sendType = (info.healthDepartmentInfo.sendType || info.healthDepartmentInfo.sendType == '0')? info.healthDepartmentInfo.sendType : '0'"></span>
                                        <select class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.sendType" disabled>
                                            <option ng-selected="info.healthDepartmentInfo.sendType == x.id" ng-value="x.id" ng-repeat="x in sendHeTypeList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">三日内医疗费</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.thrMediFeError}">
                                        <input type="text" maxlength="40" class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.thrMediFe" placeholder="请输入三日内医疗费" float-only>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">后期医疗费</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.healthDepartmentInfo.laterMedicalFeeError}">
                                        <input type="text" maxlength="40" class="form-control ng-pristine ng-valid" ng-model="info.healthDepartmentInfo.laterMedicalFee" placeholder="请输入后期医疗费" float-only>
                                    </div>
                                </div>
                                <div class="form-group col-xs-12">
                                    <div class="col-lg-2 col-xs-2 text-right">
                                        <label class=" control-label">伤情诊断</label>
                                    </div>
                                    <div class="col-lg-1 col-xs-1" style="margin-right: 20px;">
                                        <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ngf-multiple="true" ngf-pattern=".doc,.docx,image/*,.pdf" accept="image/gif,image/jpeg,image/jpg,image/png,image/svg,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document,application/pdf" ngf-select="addEvidence($files, info.healthDepartmentInfo)"  ng-click="checkflash()" ng-model="files">
                                            <i class="iconfont icon-shangchuan brand-color" style="color: #fff;"></i>
                                            上传
                                        </button>
                                    </div>
                                    <div class="col-lg-1 col-xs-1" style="font-size: 16px;width: 110px; border-left: 1px solid #E4E4E4;padding-left: 30px;" ng-if="info.healthDepartmentInfo.enclosureInfoList.length > 0">
                                        <label class="checkbox-inline">
                                            <input type="checkbox" ng-checked="info.healthDepartmentInfo.isAllChecked" ng-click="handleAllChecked($event, info.healthDepartmentInfo.enclosureInfoList, info.healthDepartmentInfo)">全选
                                        </label>
                                    </div>
                                    <div class="col-lg-1 col-xs-1" ng-if="info.healthDepartmentInfo.enclosureInfoList.length > 0 && info.healthDepartmentInfo.enclosureArr.length">
                                        <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ng-click="deleteEnclosure(info.healthDepartmentInfo)">
                                            删除
                                        </button>
                                    </div>
                                </div>
                                <div class="form-group col-xs-12">
                                    <div class="col-lg-2 col-xs-2 text-right"></div>
                                    <div class="col-lg-10 col-xs-10">
                                        <ul class="evidences">
                                            <li ng-repeat="enclosureInfo in info.healthDepartmentInfo.enclosureInfoList">
                                                <i ng-click="handleEnclosure(enclosureInfo, info.healthDepartmentInfo.enclosureInfoList, info.healthDepartmentInfo)" class="{{enclosureInfo.isChecked? 'icon-active':'icon-normal'}}"></i>
                                                <!--<img ng-src="{{enclosureInfo.fileType == '0'? imageAddress+enclosureInfo.path : 'views/images/police-evidence-default.png'}}" alt="">-->
                                                <img ng-show="enclosureInfo.fileType == '0'" ng-src="{{imageAddress+enclosureInfo.path}}" ng-click="zoomImage(bigPictureUrl+enclosureInfo.path)">
                                                <a ng-click="downloadFile(enclosureInfo)">
                                                    <img ng-show="enclosureInfo.fileType == '1'" ng-src="{{'views/images/police-evidence-default1.png'}}" alt="">
                                                    <img ng-show="enclosureInfo.fileType == '2'" ng-src="{{'views/images/police-evidence-default2.png'}}" alt="">
                                                </a>
                                                <p>{{enclosureInfo.oldname}}</p>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-xs-12 row clearfix"  style="text-align: center;margin-top: 10px;">
                                    <button type="button" class="btn btn-brand"  ng-if="!info.healthDepartmentInfo.sendType||info.healthDepartmentInfo.sendType=='0'"  ng-click="sendHealth(info)">发送医院</button>
                                    <button type="button" class="btn btn-brand"  ng-if="info.healthDepartmentInfo.sendType=='1'" disabled>已发送医院</button>
                                    <button type="button" class="btn btn-brand"  ng-if="info.healthDepartmentInfo.sendType=='2'" disabled>已完成补录</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!--工伤信息-->
        <div style="border: 1px solid #ddd;" class="module-box bg-default" style="padding-bottom: 10px;">
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <span class="h1_line"></span>
                    工伤信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><span class="Required">*</span> 主管部门</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.indMedInfo.indInjuryDepartmentError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="info.indMedInfo.indInjuryDepartment" ng-change="getValueById(info.indMedInfo.indInjuryDepartment, info.indMedInfo, 'indInjuryDep', (appraisalMechanismList | filter:{category : '#07'}), 'relationOrgName', true)">
                                        <option ng-selected="info.indMedInfo.indInjuryDepartment == x.relationOrgId" ng-value="x.relationOrgId" ng-repeat="x in (appraisalMechanismList | filter:{category : '#07'})">{{x.relationOrgName}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label">补充状态</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.indMedInfo.sendTypeError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="info.indMedInfo.sendType" disabled>
                                        <option ng-selected="info.indMedInfo.sendType == x.id" ng-value="x.id" ng-repeat="x in sendTypeList">{{x.value}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label">反馈时间</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.indMedInfo.sendDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="info.indMedInfo.sendDate" is-open="false" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天" />
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openIndRegisterTime($event, info.indMedInfo)" disabled><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6 form-group form-group-sm">
                                <div class="col-xs-12">
                                    <div>
                                        <label class="col-lg-4 col-xs-5 text-right  control-label">是否符合工伤要求</label>
                                        <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.indMedInfo.indPastMusterError}">
                                            <label class="radio-inline">
                                                <input type="radio" name="indPastMuster" ng-model="info.indMedInfo.indPastMuster" value="1">是
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="indPastMuster" ng-model="info.indMedInfo.indPastMuster" value="0">否
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
            </div>

            <!--医保信息-->
            <div class="nav-line-left">
                <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                    <span class="h1_line"></span>
                    医保信息
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><span class="Required">*</span> 主管部门</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.indMedInfo.medInsuranceDepartmentError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="info.indMedInfo.medInsuranceDepartment" ng-change="getValueById(info.indMedInfo.medInsuranceDepartment, info.indMedInfo, 'medInsuranceDep', (appraisalMechanismList | filter:{category : '#07'}), 'relationOrgName', true)">
                                        <option ng-selected="info.indMedInfo.medInsuranceDepartment == x.relationOrgId" ng-value="x.relationOrgId" ng-repeat="x in (appraisalMechanismList | filter:{category : '#07'})">{{x.relationOrgName}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label">补充状态</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': info.indMedInfo.sendTypeError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="info.indMedInfo.sendType" disabled>
                                        <option ng-selected="info.indMedInfo.sendType == x.id" ng-value="x.id" ng-repeat="x in sendTypeList">{{x.value}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label">反馈时间</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.indMedInfo.sendDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="info.indMedInfo.sendDate" is-open="false" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openMedRegisterTime($event, info.indMedInfo)" disabled><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6 form-group form-group-sm">
                                <div class="col-xs-12">
                                    <div>
                                        <label class="col-lg-4 col-xs-5 text-right  control-label">是否符合医保要求</label>
                                        <div class="col-lg-8 col-xs-7" ng-class="{'has-error':info.indMedInfo.medPastMusterError}">
                                            <label class="radio-inline">
                                                <input type="radio" ng-model="info.indMedInfo.medPastMuster" ng-checked="info.indMedInfo.medPastMuster == '1'" value="1">是
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" ng-model="info.indMedInfo.medPastMuster" ng-checked="info.indMedInfo.medPastMuster == '0'" value="0">否
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;margin-top: 10px;">
                                <button type="button" class="btn btn-brand"  ng-if="info.indMedInfo.sendType=='2'" disabled>已完成补录</button>
                                <button type="button" class="btn btn-brand"  ng-if="info.indMedInfo.sendType=='0'||info.indMedInfo.sendType==null"  ng-click="sendIndMed(info)">发送补充</button>
                                <button type="button" class="btn btn-brand"  ng-if="info.indMedInfo.sendType =='1'" disabled>已发送补充</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div ng-show="(!police.accidentInfo.id && police.accidentInfo.id != '0') || !police.jpoliceVehicVO[0].policePersonInformation.id" style="margin-top: 50px;text-align: center;">
    <p>请先填写并保存当事人信息！</p>
</div>
<div ng-show=" (police.jpoliceVehicVO | isWounded).length == '0'" style="margin-top: 50px;text-align: center;">
    <p>暂无伤者信息！</p>
</div>
<div id="big_img_box">
    <div class="big_img_box_bj"></div>
    <div class="big_img_box_img_box">
        <div id="big_img_box_img_f" style="position: absolute;">
            <div id="big_img_box_img_bar" style="width: 100%; height: 100%;" title="可以拖拽移动"></div>
            <img id="big_img_box_img" src="imgSrc">
        </div>
        <div class="big_img_box_bottom">
            <div class="big_img_box_bottom_bg"></div>
            <i class="iconfont icon-qiepian30" onclick="rotateImg();"></i>
            <i class="iconfont icon-iconfontfangda" onclick="enlargeimg();"></i>
            <i class="iconfont icon-iconfontsuoxiao" onclick="narrowimg();"></i>
        </div>
        <i class="iconfont icon--tingqiantiaojieduihao-Xhao" onclick="hideImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy-copy" onclick="leftImg();"></i>
        <i class="iconfont icon-zuoyoujiantou-copy-copy" onclick="rightImg();"></i>
    </div>
</div>
<script type="text/javascript">
  var oBox = document.getElementById("big_img_box_img_f");
  var oBar = document.getElementById("big_img_box_img_bar");
  startDrag(oBar, oBox);
