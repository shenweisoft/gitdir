<!-- <i class="iconfont pull-right icon-guanbi close" ng-click="cancel()" style="margin-right: 10px; margin-top: 10px;"></i> -->
<div class="modal-header ng-scope" style="font-weight: bold;">
    字段说明
    <i class="iconfont pull-right icon-guanbi close" ng-click="cancel()"></i>
</div>
<div class="row">
	<div class="col-xs-12" style="margin-top: 10px;">
		<div class="col-xs-6">
			<table class="table table-bordered">
			    <tr>
			        <th>字段KEY</th>
			        <th>字段VALUE</th>
			    </tr>
			    <tr ng-repeat="keyConstant in keyConstantList">
			        <td>{{keyConstant.key}}</td>
			        <td>{{keyConstant.value}}</td>
			    </tr>
			</table>
		</div>
		<div class="col-xs-6">
			<table class="table table-bordered">
				<tr>
			        <th>字段id</th>
			        <th>字段VALUE</th>
			    </tr>
				<tr ng-repeat="doc in docList">
			        <td>{{doc.id}}</td>
			        <td>{{doc.value}}</td>
			    </tr>
			</table>
		</div>
	</div>
</div>