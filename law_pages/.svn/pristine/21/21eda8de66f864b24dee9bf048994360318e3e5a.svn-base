<div class="bg-default" style="margin-left: -15px; margin-right: -15px;">
	<div class=" panel-heading">
		<div class="row">
			<h4 class="col-xs-4 col-lg-7"><span class="h1_line"></span>
				<span ng-if="url == 'allCaseList'">案件查询</span>
				<span ng-if="url == 'adjustNeeddTodoList'">待办事项</span>
			</h4>
			<div class="row col-xs-8 col-lg-5">
				<div class="col-xs-12 line-height40">
					<i class="iconfont icon-shaixuan1 cur-pointer pull-right " style="font-size: 24px; color: #3F65D6;" ng-click=" filterShow = !filterShow"></i>
					<button type="button" class="btn btn-brand pull-right" ng-show="!filterShow" ng-click="queryAll()" style="margin-right: 15px; margin-left: 20px; margin-top: 3px;">搜索</button>
					<div class="search_box pull-right" ng-show="!filterShow" style="width: 212px;">
						<input type="text" class="input-xs search_input" placeholder="请输入流水号，调解号，案号" ng-model="search.overall">
						<i class="iconfont icon-sousuo input-search-img"></i>
					</div>
				</div>
			</div>
		</div>
		<div ng-show="filterShow">
			<form role="form" class="form-horizontal form-bg-color padding-20">
				<div class="row clearfix form-row-bottom">
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 text-right no-padding">流水号</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入流水号" ng-model="search.serialNo">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding">案号</label>
							<div class="col-xs-9">
								<input type="text" class="form-control input" placeholder="请输入调解号/案号" ng-model="search.lawNo">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding">案件类型</label>
							<div class="col-xs-9">
								<select class="form-control no-padding " ng-model="search.operateType" style="text-indent: 8px;">
									<option value="">==请选择==</option>
									<option value="0">司法案件</option>
									<option value="1">诉讼案件</option>
									<option value="4">调解案件</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 案由</label>
							<div class="col-xs-9">
								<select class="form-control no-padding " ng-model="search.reason" style="text-indent: 8px;">
									<option value="">==请选择==</option>
									<option ng-repeat="item in factTypeList" value="{{item.id}}">{{item.value}}</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 当事人</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入当事人" ng-model="search.applicantOrRespondent">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 调解员</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入调解员" ng-model="search.adjustName">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 承办法官</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入承办法官" ng-model="search.judgeName">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 书记员</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入书记员" ng-model="search.clerkName">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 调解中心</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入调解中心" ng-model="search.adjustOrgName">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class="control-label col-xs-3 no-padding"> 立案法院</label>
							<div class="col-xs-9">
								<input type="text" class="form-control" placeholder="请输入立案法院" ng-model="search.lawOrgName">
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4 col-xs-6">
						<div class="form-group">
							<label class=" control-label col-xs-3 no-padding">状态</label>
							<div class="col-xs-9">
								<select class="form-control no-padding form" ng-model="search.state" style="text-indent: 8px;">
									<option value="">==请选择==</option>
									<option ng-repeat="item in lawStateList | stateFilter " value="{{item.id}}">{{item.value}}</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-4  col-xs-6" style="margin-left: 1px">
						<div class="form-group">
							<label class=" control-label col-xs-3 no-padding">选择日期</label>
							<div class="col-xs-9">
								<select class="form-control no-padding form" ng-model="dateType">
									<option value="">==请选择==</option>
									<option value="adjustDate">调解日期</option>
									<option value="filingDate">立案日期</option>
									<option value="courtDate">开庭日期</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-lg-6 col-xs-5 no-padding-r">
						<div class="col-xs-6 no-padding-l">
							<div class="input-group">
								<input type="text" readonly class="form-control" placeholder="请选择开始日期" datepicker-popup="yyyy-MM-dd" ng-model="startDate" is-open="dateOpen.opened" show-button-bar="false">
								<span class="input-group-btn">
									<button type="button" class="btn btn-default" ng-click="open($event,'opened')">
										<i class="glyphicon glyphicon-calendar"></i>
									</button>
								</span>
							</div>
						</div>
						<div class="col-xs-6 no-padding-l">
							<span class="cross-bar flo-r"></span>
							<div class="input-group">
								<input type="text" readonly class="form-control" placeholder="请选择结束日期" datepicker-popup="yyyy-MM-dd" ng-model="endDate" is-open="dateOpen.opened1" show-button-bar="false">
								<span class="input-group-btn">
									<button type="button" class="btn btn-default" ng-click="open($event,'opened1')">
										<i class="glyphicon glyphicon-calendar"></i>
									</button>
								</span>
							</div>
						</div>
					</div>
				</div>
				<div class="row clearfix form-row-bottom">
					<div class="col-xs-12 text-center">
						<button class="btn btn-default" ng-click="filterShow = false">取消</button>
						<button class="btn btn-default" style="margin: 0 10px;" type="reset" ng-click="reset()">重置</button>
						<button class="btn btn-brand" ng-click="queryAllCaseList()">查询</button>

					</div>
				</div>
			</form>
		</div>
	</div>
	<div ng-if="blankShow && pageData.showList.length == 0" class="text-center">
		<img src="views/images/search_null.png" ><br>
		抱歉，未找到相关内容！
	</div>
	<div class="panel-body no-padding-v" style="position: relative;">
		<div class="right_checkbox_list"><i style="cursor: pointer;" class="iconfont icon-icon"></i>
			<div class="checkbox_list" style="margin-left: -35px;">
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox1" ng-model="fo.serialNoShow" ng-checked="fo.serialNoShow"  ng-change="listChange('serialNoShow')"> <label for="checkbox1">流水号</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox2" ng-model="fo.lawNoShow" ng-checked="fo.lawNoShow"  ng-change="listChange('lawNoShow')"> <label for="checkbox2">案号</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox3" ng-model="fo.operateTypeShow" ng-checked="fo.operateTypeShow"  ng-change="listChange('operateTypeShow')"> <label for="checkbox3">立案类型</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox4" ng-model="fo.adjustDateShow" ng-checked="fo.adjustDateShow"  ng-change="listChange('adjustDateShow')"> <label for="checkbox4">调解日期</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox5" ng-model="fo.filingDateShow" ng-checked="fo.filingDateShow"  ng-change="listChange('filingDateShow')"> <label for="checkbox5">立案日期</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox6" ng-model="fo.courtDateShow" ng-checked="fo.courtDateShow"  ng-change="listChange('courtDateShow')"> <label for="checkbox6">开庭日期</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox7" ng-model="fo.applicantShow" ng-checked="fo.applicantShow"  ng-change="listChange('applicantShow')"> <label for="checkbox7">当事人</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox21" ng-model="fo.adjustNameShow" ng-checked="fo.adjustNameShow"  ng-change="listChange('adjustNameShow')"> <label for="checkbox21">调解员</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox8" ng-model="fo.chiefNameShow" ng-checked="fo.chiefNameShow"  ng-change="listChange('chiefNameShow')"> <label for="checkbox8">承办法官</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox9" ng-model="fo.clerkNameShow" ng-checked="fo.clerkNameShow"  ng-change="listChange('clerkNameShow')"> <label for="checkbox9">书记员</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox10" ng-model="fo.adjustOrgNameShow" ng-checked="fo.adjustOrgNameShow"  ng-change="listChange('adjustOrgNameShow')"> <label for="checkbox10">调解中心</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox11" ng-model="fo.lawOrgNameShow" ng-checked="fo.lawOrgNameShow"  ng-change="listChange('lawOrgNameShow')"> <label for="checkbox11">立案法院</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox12" ng-model="fo.stateShow" ng-checked="fo.stateShow" ng-change="listChange('stateShow')"> <label for="checkbox12">案件状态</label></div>
				<div class="checkbox_list_e"><input type="checkbox" id="checkbox13" ng-model="fo.lawMoneyShow" ng-checked="fo.lawMoneyShow"  ng-change="listChange('lawMoneyShow')"> <label for="checkbox13">案件金额</label></div>
			</div>
		</div>
		<table class="table table-striped-info table-hover table-fixed" ng-if="pageData.showList.length > 0">
			<thead>
			<tr>
			  <th ng-if="fo.serialNoShow">流水号</th>
				<th ng-if="fo.lawNoShow">案号</th>
				<th ng-if="fo.operateTypeShow">立案类型</th>
				<th ng-if="fo.adjustDateShow">调解日期</th>
				<th ng-if="fo.filingDateShow">立案日期</th>
				<th ng-if="fo.courtDateShow">开庭日期</th>
				<th ng-if="fo.applicantShow">当事人</th>
				<th ng-if="fo.adjustNameShow">调解员</th>
				<th ng-if="fo.chiefNameShow">承办法官</th>
				<th ng-if="fo.clerkNameShow">书记员</th>
				<th ng-if="fo.adjustOrgNameShow">调解中心</th>
				<th ng-if="fo.lawOrgNameShow">立案法院</th>
				<th ng-if="fo.stateShow">案件状态</th>
				<th ng-if="fo.lawMoneyShow">案件金额</th>

			</tr>
			</thead>
			<tbody>
			<tr ng-repeat="case in pageData.showList ">
			  <td ng-if="fo.serialNoShow"><a class="cur-pointer" ng-click="goCase(case)" title="点击查询详细">{{case.serialNo}}</a></td>
				<td ng-if="fo.lawNoShow">{{case.lawNo}}</td>
				<td ng-if="fo.operateTypeShow"><span ng-if="case.operateType == 1">诉讼案件</span><span ng-if="case.operateType == 0 && case.adjustResult != 4">司法确认案件</span><span ng-if="case.operateType == 0 && case.adjustResult == 4">调解案件</span></td>
				<td ng-if="fo.adjustDateShow">{{case.adjustDate | stringDate | date: 'yyyy-MM-dd' | valueNull}}</td>
				<td ng-if="fo.filingDateShow">{{case.filingDate | stringDate | date: 'yyyy-MM-dd' | valueNull}}</td>
				<td ng-if="fo.courtDateShow">{{case.courtDate | stringDate | date: 'yyyy-MM-dd HH:mm:ss' | valueNull}}</td>
				<td ng-if="fo.applicantShow">{{case.applicant | valueNull}}</td>
				<td ng-if="fo.adjustNameShow">{{case.adjustName | valueNull}}</td>
				<td ng-if="fo.chiefNameShow">{{case.chiefName | valueNull}}</td>
				<td ng-if="fo.clerkNameShow">{{case.clerkName | valueNull}}</td>
				<td ng-if="fo.adjustOrgNameShow">{{case.adjustOrgName | valueNull}}</td>
				<td ng-if="fo.lawOrgNameShow">{{case.lawOrgName | valueNull}}</td>
				<td ng-if="fo.stateShow">{{case.state | stateChangeText:lawStateList | valueNull}}</td>
				<td ng-if="fo.lawMoneyShow">{{case.lawMoney | currency | valueNull}}</td>
			</tr>
			</tbody>
		</table>
		<div class="text-center" ng-if="search.totalItems">
			<pagination previous-text="<" next-text=">" first-text="<<" last-text=">>" items-per-page="itemNum" class="pagination-custom" total-items="search.totalItems" ng-model="search.pageNo" ng-change="pageChanged()" max-size="7" boundary-links="true" rotate="false"></pagination>
		</div>
	</div>

</div>
