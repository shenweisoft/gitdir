<div class="role_management_d relative_top">
  <div class="row bg-default">
    <div class="col-xs-12">
        <div class="radio" style="margin-left: 20px;">
          <label style="margin-right: 20px;" class="ng-scope">
            <input type="radio" ng-model="jyEnvironmentInfo.environmentType" ng-checked="jyEnvironmentInfo.environmentType == 1" class="ng-pristine ng-valid" name="config" value="1" >测试
          </label>
          <label style="margin-right: 20px;" class="ng-scope">
            <input type="radio" ng-model="jyEnvironmentInfo.environmentType" ng-checked="jyEnvironmentInfo.environmentType == 2" class="ng-pristine ng-valid" name="config" value="2" >百度云
          </label>
          <label style="margin-right: 20px;" class="ng-scope">
            <input type="radio" ng-model="jyEnvironmentInfo.environmentType" ng-checked="jyEnvironmentInfo.environmentType == 3" class="ng-pristine ng-valid" name="config" value="3" >首信云
          </label>
          <button class="btn btn-brand" ng-click="insertOrUpdateJyEnvironmentInfo()">确定</button>
        </div>
    </div>
  </div>
</div>