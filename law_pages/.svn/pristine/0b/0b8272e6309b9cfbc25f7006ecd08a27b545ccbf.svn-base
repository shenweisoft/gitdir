<div class="row header_e no-margin" ng-hide="isSelf== '1'">
	<div class="col-xs-11 no-padding">
		<!--<div class="col-xs-1 text-align-center">
			<img class="Court_logo" src="views/images/logo.png">
		</div>-->
		<div class="logo-content">
			<img class="logo-content-img" src="views/images/qweasd.png">
			<span class="logo-content-text">{{domainName}}</span>
		</div>
		<!--  <p class="col-xs-11 Court_name no-padding">{{adjust.lawOrgName}}<span> | </span><span>案件卷宗详情</span></p>-->
	</div>
</div>
<div class="case-details-head bg-default font-size13" ng-hide="isSelf== '1'">
	<div head-detail></div>
</div>
<div class="row no-margin" style="background:#ecf0f3; min-height: 500px;" ng-class="isSelf=='1'?'trafficPolice':''">

	<div class="col-xs-2" ng-if="querySubType != '1'" style="padding-right: 0px; padding-left: 10px; padding-top: 9px; height: 100%;">
	  <div class="col-xs-12 background_color_d Evidence_list_cont_l" style="height: 100%;">
		<div class="Evidence_list_cont_l_e" ng-class="{'active':stage == stageList.adjustState}" ng-click="queryStage(stageList.adjustState)">
			<div class="pull-left "></div>
			<p><i class="iconfont icon--suqiantiaojie"></i>   调解阶段</p>
		</div>
		<div class="Evidence_list_cont_l_e" ng-class="{'active':stage == stageList.litigationState}" ng-if="law.operateType == '1'" ng-click="queryStage(stageList.litigationState)">
			<div class="pull-left "></div>
			<p><i class="iconfont icon--faqisusong"></i>   诉讼阶段</p>
		</div>
		<div class="Evidence_list_cont_l_e"  ng-class="{'active':stage == stageList.handlingState}" ng-if="(law.state != '1000' && law.state != '1001' && law.state != '1002') || (law.state == '1002' && law.adjustResult == '0' )|| (law.operateType == '1' && law.state == '1002')" ng-click="queryStage(stageList.handlingState)">
			<div class="pull-left "></div>
			<p><i class="iconfont icon--banan"></i>   办案阶段</p>
		</div>
      </div>
  	</div>

	<div class="col-xs-10 row_margin" style="margin-left: -5px; padding-right: 0; padding-bottom: 20px;">
		<div class="col-xs-12 bg-default margin-bottom-10 case-details-head">
			<!--调解阶段和诉讼阶段-->
			<ul class="list-inline nav_list no-margin" ng-show="stage == stageList.adjustState || stage == stageList.litigationState">
				<li ng-class="{'border_blue': fileType == 'proposer'}" ng-click="fileType = 'proposer'">
					<span ng-if="stage == stageList.adjustState">申请人信息</span>
					<span ng-if="stage == stageList.litigationState">原告信息</span>
				</li>
				<li ng-class="{'border_blue': fileType == 'respondent'}"  ng-click="fileType = 'respondent'">
					<span ng-if="stage == stageList.adjustState">被申请人信息</span>
					<span ng-if="stage == stageList.litigationState">被告信息</span>
				</li>
				<li ng-class="{'border_blue': fileType == 'reason'}"  ng-click="fileType = 'reason'">事实理由</li>
				<li ng-class="{'border_blue': fileType == 'adjustInfor'}"  ng-click="fileType = 'adjustInfor'">
					<span ng-if="stage == stageList.adjustState">调解信息</span>
					<span ng-if="stage == stageList.litigationState">诉请信息</span>
				</li>
				<!--只有达成调解才有调解协议-->
				<li ng-class="{'border_blue': fileType == 'adjustAgreement'}" ng-click="fileType = 'adjustAgreement'" ng-show="stage == stageList.adjustState && ( adjust.adjustResult=='0' || adjust.adjustResult=='1')">调解协议</li>
				<li ng-class="{'border_blue': fileType == 'lawComplaint'}"  ng-click="fileType = 'lawComplaint'" ng-show="stage == stageList.litigationState">起诉状</li>
			</ul>
			<ul class="list-inline nav_list no-margin" ng-show="stage == stageList.handlingState">
				<li ng-class="{'border_blue': fileType == 'approval'}" ng-click="fileType = 'approval'">审批</li>
				<li ng-class="{'border_blue': fileType == 'filing'}" ng-click="fileType = 'filing'">立案</li>
				<li ng-class="{'border_blue': fileType == 'division'}" ng-click="fileType = 'division'">分案</li>
			</ul>
		</div>
		<!--申请人法人信息-->
		<div  ng-if="((fileType == 'proposer' && applicant.personType == 0) || (fileType == 'respondent' && applicant.personType == 1) )" ng-repeat="applicant in adjust.applicantArray" style="margin-bottom: 30px;">
			<div class="col-xs-12 bg-default" id="{{'name' + applicant.hashIndex}}">
				<div class="col-xs-12 nav-head">
					<h4><i class="iconfont icon-shenqing"></i>
						<span ng-if="applicant.personType == 0">
              <span ng-if="stage == stageList.adjustState">申请人：</span>
					    <span ng-if="stage == stageList.litigationState">原告：</span>
            </span>
						<span ng-if="applicant.personType == 1">
              <span ng-if="stage == stageList.adjustState">被申请人：</span>
					    <span ng-if="stage == stageList.litigationState">被告：</span>
            </span>
						<span ng-if="applicant.idType == 0">{{applicant.personName}}</span>
						<span ng-if="applicant.idType != 0">{{applicant.orgName}}</span>
					</h4>
				</div>
			</div>
			<!--公民身份类型-->
			<div class="col-xs-12 bg-default no-margin" ng-show="applicant.idType == 0" style="/*margin-bottom: 10px;*/ padding-bottom: 40px; height: 100%;">
				<div class="col-xs-8 col-sm-7 col-md-6 col-lg-5 row_margin">
					<div class="col-xs-12 form-group margin_top">
						<div class="col-xs-5 text-right text-muted">身份类型：</div>
						<div class="col-xs-7 no-padding">{{applicant.idType | id2Text:idTypeList}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">
							<span ng-if="applicant.personType == 0">
                <span ng-if="stage == stageList.adjustState">申请人姓名：</span>
					      <span ng-if="stage == stageList.litigationState">原告姓名：</span>
              </span>
							<span ng-if="applicant.personType == 1">
                <span ng-if="stage == stageList.adjustState">被申请人姓名：</span>
					      <span ng-if="stage == stageList.litigationState">被告姓名：</span>
              </span>
						</div>
						<div class="col-xs-7 no-padding">{{applicant.personName}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">证件类型：</div>
						<div class="col-xs-7 no-padding">{{applicant.certificatesType | id2Text:certificatesTypeList}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">证件号码：</div>
						<div class="col-xs-7 no-padding">{{applicant.idNo}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">性别：</div>
						<div class="col-xs-7 no-padding">
							<span  ng-if="applicant.sex == 0">男</span>
							<span  ng-if="applicant.sex == 1">女</span>
						</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">出生日期：</div>
						<div class="col-xs-7 no-padding">{{applicant.birthDay | date:'yyyy-MM-dd'}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">民 族：</div>
						<div class="col-xs-7 no-padding">{{applicant.nation}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">电话号码：</div>
						<div class="col-xs-7 no-padding">{{applicant.telephone}}</div>
					</div>
					<!--<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">电子邮箱：</div>
						<div class="col-xs-7 no-padding">{{applicant.email}}</div>
					</div>-->
					<div ng-show="applicant.personType == 1">
						<div class="col-xs-12 form-group" >
							<div class="col-xs-5 text-right text-muted">车牌号：</div>
							<div class="col-xs-7 no-padding">{{applicant.plateNo}}</div>
						</div>
						<div class="col-xs-12 form-group" >
							<div class="col-xs-5 text-right text-muted">承保保险公司：</div>
							<div class="col-xs-7 no-padding">{{applicant.companyName}}</div>
						</div>
						<div class="col-xs-12 form-group" >
							<div class="col-xs-5 text-right text-muted">报案号：</div>
							<div class="col-xs-7 no-padding">{{applicant.lossNo}}</div>
						</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">户籍所在地：</div>
						<div class="col-xs-7 no-padding">{{applicant.domicile}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">居住地：</div>
						<div class="col-xs-7 no-padding">{{applicant.residence}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted no-padding">连续居住已满一年：</div>
						<div class="col-xs-7 no-padding">
							<span ng-if="applicant.isLive != 1">否</span>
							<span ng-if="applicant.isLive == 1">是</span>
						</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-5 text-right text-muted">送达地址：</div>
						<div class="col-xs-7 no-padding">{{applicant.sendAddress}}</div>
					</div>
				</div>
				<div class="col-xs-4 col-sm-5 col-md-6 col-lg-7 row_margin">
					<div class="col-xs-12"><br><br></div>
					<div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
						<div class="margin_top img_box_boder Front">
							<img class="id_img" ng-if="applicant.certificatesType == '0'" ng-click="zoomImage(applicant.idFacePicture.indexOf(defaultImg3)==-1?bigPictureUrl+applicant.idFacePicture:defaultImg3)" ng-src="{{applicant.idFacePicture.indexOf(defaultImg3)==-1?imageAddress+applicant.idFacePicture:defaultImg3}}">
							<img class="id_img" ng-if="applicant.certificatesType != '0'" ng-click="zoomImage(applicant.idFacePicture.indexOf(defaultImg)==-1?bigPictureUrl+applicant.idFacePicture:defaultImg)" ng-src="{{applicant.idFacePicture.indexOf(defaultImg)==-1?imageAddress+applicant.idFacePicture:defaultImg}}">
							<div class="img_bottom_bar">
								<p><span ng-if="applicant.certificatesType==0">证件照身份证号面</span><span ng-if="applicant.certificatesType!=0">{{applicant.certificatesType|certificatesTypeToText:certificatesTypeList}}正面</span></p>
							</div>
						</div>
					</div>
					<div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
						<div class=" margin_top img_box_boder Back">
							<img class="id_img" ng-if="applicant.certificatesType == '0'" ng-click="zoomImage(applicant.idBackPicture.indexOf(defaultImg2)==-1?bigPictureUrl+applicant.idBackPicture:defaultImg2)" ng-src="{{applicant.idBackPicture.indexOf(defaultImg2)==-1?imageAddress+applicant.idBackPicture:defaultImg2}}">
							<img class="id_img" ng-if="applicant.certificatesType != '0'" ng-click="zoomImage(applicant.idBackPicture.indexOf(defaultImg)==-1?bigPictureUrl+applicant.idBackPicture:defaultImg)" ng-src="{{applicant.idBackPicture.indexOf(defaultImg)==-1?imageAddress+applicant.idBackPicture:defaultImg}}">
							<div class="img_bottom_bar">
								<p><span ng-if="applicant.certificatesType==0">证件照国徽面</span><span ng-if="applicant.certificatesType!=0">{{applicant.certificatesType|certificatesTypeToText:certificatesTypeList}}反面</span></p>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!--公民结束-->
			<!--法人或者其他组织身份类型-->
			<div class="row no-margin" ng-show="applicant.idType != 0">
				<div class="col-xs-12 bg-default">
				<div class="col-xs-8 col-sm-7 col-md-6 col-lg-5 row_margin">
					<div class="col-xs-12 form-group margin_top">
						<div class="col-xs-4 text-right text-muted">身份类型：</div>
						<div class="col-xs-8 no-padding">{{applicant.idType | id2Text:idTypeList}}</div>
					</div>
					<!--被申请人法人时选择-->
					<div class="col-xs-12 form-group" ng-show="applicant.personType == 1 && applicant.idType == 1">
						<div class="col-xs-4 text-right text-muted">类型：</div>
						<div class="col-xs-8 no-padding">{{applicant.enterpriseType| id2Text:companyTypeList}}</div>
					</div>

					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">
							<span ng-show="applicant.idType==1">企业名称：</span>
							<span ng-show="applicant.idType==2">组织名称：</span>
						</div>
						<div class="col-xs-8 no-padding">{{applicant.orgName}}</div>
					</div>
					<!--如果为被申请人并且为法人的判断-->
					<div ng-show="applicant.personType == 1 && applicant.idType == 1">
						<!--如果为保险公司类型-->
						<div ng-show="applicant.enterpriseType == 1">
							<div class="col-xs-12 form-group">
								<div class="col-xs-4 text-right text-muted">承保保险公司：</div>
								<div class="col-xs-8 no-padding">{{applicant.companyName}}</div>
							</div>
							<div class="col-xs-12 form-group">
								<div class="col-xs-4 text-right text-muted">报案号：</div>
								<div class="col-xs-8 no-padding">{{applicant.lossNo}}</div>
							</div>
						</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">代表人类型：</div>
						<div class="col-xs-8 no-padding">
							{{applicant.legalType | id2Text:legalTypeList}}
						</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">代表人姓名：</div>
						<div class="col-xs-8 no-padding">{{applicant.legalName}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">公司职务：</div>
						<div class="col-xs-8 no-padding">{{applicant.job}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">组织代码：</div>
						<div class="col-xs-8 no-padding">{{applicant.orgCode}}</div>
					</div>
					<div class="col-xs-12 form-group" ng-show="applicant.idType==1">
						<div class="col-xs-4 text-right text-muted">工商注册号：</div>
						<div class="col-xs-8 no-padding">{{applicant.registerCode}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">电话号码：</div>
						<div class="col-xs-8 no-padding">{{applicant.telephone}}</div>
					</div>
					<!--<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">电子邮箱：</div>
						<div class="col-xs-8 no-padding">{{applicant.email}}</div>
					</div>-->
					<div class="col-xs-12 form-group" ng-show="applicant.personType == 1">
						<div class="col-xs-4 text-right text-muted">车牌号：</div>
						<div class="col-xs-8 no-padding">{{applicant.plateNo}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">所住地址：</div>
						<div class="col-xs-8 no-padding">{{applicant.residence}}</div>
					</div>
					<div class="col-xs-12 form-group">
						<div class="col-xs-4 text-right text-muted">送达地址：</div>
						<div class="col-xs-8 no-padding">{{applicant.sendAddress}}</div>
					</div>
				</div>
				<div class="col-xs-4 col-sm-5 col-md-6 col-lg-7 row_margin">
					<div class="col-xs-12"><br><br></div>
					<div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
						<div class="margin_top img_box_boder Front">
							<img class="id_img" ng-click="zoomImage(applicant.businessLicensePicture.indexOf(defaultImg)==-1?bigPictureUrl + applicant.businessLicensePicture:applicant.businessLicensePicture)"  ng-src="{{applicant.businessLicensePicture.indexOf(defaultImg)==-1?imageAddress + applicant.businessLicensePicture:applicant.businessLicensePicture}}">
							<div class="img_bottom_bar">
								<p>营业执照</p>
							</div>
						</div>
					</div>
					<div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
						<div class=" margin_top img_box_boder Back">
							<img class="id_img"  ng-click="zoomImage(applicant.legalPersonPicture.indexOf(defaultImg)==-1?bigPictureUrl + applicant.legalPersonPicture:applicant.legalPersonPicture)"  ng-src="{{applicant.legalPersonPicture.indexOf(defaultImg)==-1?imageAddress + applicant.legalPersonPicture:applicant.legalPersonPicture}}">
							<div class="img_bottom_bar">
								<p>法人代表情况说明</p>
							</div>
						</div>
					</div>
				</div>
				</div>
			</div>
			<!--法人或其他组织结束-->
			<!--代理人开始-->
			<form class="form-horizontal ng-pristine ng-valid">
				<div class="col-xs-12 bg-default">
					<div class="row" ng-repeat="agent in applicant.agentArray">
						<div class="col-xs-1"></div>
						<div class="col-xs-10 no-margin bg_green" id="{{'name' + agent.hashIndex}}">
							<div class="col-xs-12 form-group">
								<h4 class="col-xs-8 text-left margin_top">代理人：{{agent.agentName}}</h4>
							</div>
							<div class="col-xs-12 form-group">
								<div class="col-xs-6">
									<div class="col-xs-6 col-lg-4 text-right text-muted">代理人类型：</div>
									<div class="col-xs-6 col-lg-8 no-padding">
										<span ng-if="agent.agentType == 0">委托</span>
										<span ng-if="agent.agentType == 1">法定</span>
									</div>
								</div>
								<div class="col-xs-6">
									<div class="col-xs-6 col-lg-5 text-right text-muted">是否主要联系人：</div>
									<div class="col-xs-6 col-lg-7 no-padding">
										<span ng-if="agent.mainContacts == 0">否</span>
										<span ng-if="agent.mainContacts != 0">是</span>
									</div>
								</div>
							</div>
							<!--委托开始-->
							<div ng-if="agent.agentType == 0">
								<div class="col-xs-12 form-group">
									<div class="col-xs-6">
										<div class="col-xs-6 col-lg-4 text-right text-muted">代理人身份：</div>
										<div class="col-xs-6 col-lg-8 no-padding">
											{{agent.agentIdentity |id2Text:proxyTypeConstantList}}
										</div>
										<!--代理人为公民-->
										<div style="position: relative; left: 66%; top: -20px; height: 20px;" ng-show="agent.agentIdentity == '1'">
											<span class=" text-muted">{{agent.agentIdentityItem | id2Text:agentIdentifySubList}}</span>
										</div>
									</div>
									<div class="col-xs-6"  ng-show="agent.agentIdentity == '1'">
										<div class="col-xs-6 col-lg-5 text-right text-muted" ng-if="agent.agentIdentityItem == '1'">与当事人关系：</div>
										<div class="col-xs-6 col-lg-7 no-padding">{{agent.relation}}</div>
									</div>
								</div>
								<div class="col-xs-12 form-group">
									<div class="col-xs-6">
										<div class="col-xs-6 col-lg-4 text-right text-muted">证件类型：</div>
										<div class="col-xs-6 col-lg-8 no-padding">{{agent.certificatesType | id2Text:certificatesTypeList}}</div>
									</div>
									<div class="col-xs-6">
										<div class="col-xs-6 col-lg-5 text-right text-muted">证件号码：</div>
										<div class="col-xs-6 col-lg-7 no-padding">{{agent.idNo}}</div>
									</div>
								</div>
								<div ng-if="agent.agentIdentity==1 && agent.agentIdentityItem==1">
								  <div class="col-xs-12 form-group" >
									<div class="col-xs-6">
									  <div class="col-xs-6 col-lg-4 text-right text-muted" style="margin-right: -5px;">出生日期：</div>
									  <div class="col-xs-6 col-lg-8 no-padding">
										{{agent.birthDay|date:'yyyy-MM-dd'}}
									  </div>
									</div>
									<div class="col-xs-6">
									  <div class="col-xs-6 col-lg-5 text-right text-muted" >性别：</div>
									  <div class="col-xs-6 col-lg-7" ng-class="">
										<div class="radio" style="margin-top: -7px;">
										  {{agent.sex=='0'?'男':'女'}}
										</div>
									  </div>
									</div>
								  </div>
								  <div class="col-xs-12 form-group">
									  <div class="col-xs-6">
								  		<div class="col-xs-6 col-lg-4 text-right text-muted">民族：</div>
										<div class="col-xs-6 col-lg-8 no-padding">
										  {{agent.nation}}
										</div>
									  </div>
								  </div>
								</div>
								<div class="col-xs-12 form-group">
									<div class="col-xs-6">
										<div class="col-xs-6 col-lg-4 text-right text-muted">委托权限：</div>
										<div class="col-xs-6 col-lg-8 no-padding">{{agent.entrustPower | id2Text:proxyPermissionConstantList}}</div>
									</div>
									<div class="col-xs-6">

									</div>
								</div>
								<div class="col-xs-12 form-group">
									<div class="col-xs-2 text-right text-muted"></div>
