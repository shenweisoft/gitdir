<div class="role_management_d relative_top">
	<div class="row bg-default">
		<div class="col-xs-12 role_management_right_d">
			<div class="role_management_right_t_d">
				<div class="col-xs-7 role_management_right_t_l_d no-padding">
					<span class="tit_personal">法庭列表</span>
					<i class="iconfont icon-shudao margin-left-5 text-muted"></i>
					<span class="font-size13 text-muted">{{orgName}}</span>
				</div>
				<div class="col-xs-5">
					<i class="iconfont icon-fanhui pull-right brand-color cur-pointer" style="font-size:30px;margin-top:-10px;" onclick="javascript:history.go(-1)"></i>
					<i class="iconfont icon-shudao margin-left-5 text-muted pull-right"></i>
					<div class="role_management_right_t_r_d" style="margin-top:10px;" ng-click="newCourt()"><i class="iconfont icon-tianjia"></i>新建法庭</div>
				</div>
			</div>
			<div class="clear"></div>
			<div class="panel-body">
				<div class="table-responsive">
					<table class="table table-hover">
						<thead>
                            <tr>
                                <th class="col-xs-3">法庭名称</th>
                                <th class="col-xs-3">是否启用</th>
                                <th class="col-xs-3">排序</th>
                                <th class="col-xs-3">操作</th>
                            </tr>
						</thead>
						<tbody>
                            <tr ng-repeat="sysOrgCourt in sysOrgCourtList">
                                <td>{{sysOrgCourt.courtName}}</td>
                                <td>
                                    <span ng-if="sysOrgCourt.enable == '1'">是</span>
                                    <span ng-if="sysOrgCourt.enable == '2'">否</span>
                                </td>
                                <td>{{sysOrgCourt.sort}}</td>
                                <td class="action_bar">
                                    <i class="iconfont icon-bianji" style="font-size:18px;" ng-click="newCourt(sysOrgCourt)"></i>
                                    <i class="iconfont icon-iconfont19" style="font-size:18px;" ng-click="deleteCourt(sysOrgCourt)"></i>
                                </td>
                            </tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>