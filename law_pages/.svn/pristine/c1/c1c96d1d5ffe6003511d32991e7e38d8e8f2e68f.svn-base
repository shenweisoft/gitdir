<div class="modal-header">
  <i class="iconfont icon-guanbi close" ng-click="cancel()"></i>
  <h4 class="modal-title" id="myModalLabel">{{title}}</h4>
</div>
<div class="modal-body">
  <form role="form">
    <div class="row row_margin">
      <div class="col-xs-3 text-right">
        <label class="line-height-30"><span class="xing">*</span>上级部门</label>
      </div>
      <div class="col-xs-8 line-height-30">
        <span>{{items.selectedNode.text}}</span>
      </div>
      <div class="col-xs-1"></div>
    </div>
    <div class="form-group" ng-class="{'has-error':deptNameErrorMessage}">
      <div class="row row_margin">
        <div class="col-xs-3 text-right">
          <label class="line-height-30"><span class="xing">*</span>部门名称</label>
        </div>
        <div class="col-xs-6">
          <input maxlength="30" class="form-control" name="text" ng-model="deptName"  placeholder="请输入">
        </div>
        <div class="col-xs-3"></div>
        <span class="Prompt_message" ng-bind="deptNameErrorMessage"></span>
      </div>
    </div>
    <div class="form-group">
      <div class="row row_margin" ng-class="{'has-error':deptCodeErrorMessage}">
        <div class="col-xs-3 text-right">
          <label class="line-height-30"><span class="xing">*</span>部门代码</label>
        </div>
        <div class="col-xs-6">
          <input type="text" maxlength="20" class="form-control input-sm" ng-model="deptCode" placeholder="请输入">
        </div>
        <div class="col-xs-3"></div>
        <span class="Prompt_message" ng-bind="deptCodeErrorMessage"></span>
      </div>
    </div>
    <div class="form-group">
      <div class="row row_margin" >
        <div class="col-xs-3 text-right">
          <label><span class="xing"></span>业务类型</label>
        </div>
        <div class="col-xs-6">
          <input type="text" maxlength="50" class="form-control input-sm" ng-model="deptType" name="deptType" placeholder="请输入">
        </div>
        <div class="col-xs-3"></div>
      </div>
    </div>
    <div class="form-group" ng-if="items.selectedNode.category == '#02'">
      <div class="row row_margin" >
        <div class="col-xs-3 text-right">
          <label><span class="xing"></span></label>
        </div>
        <div class="col-xs-6">
          <input type="checkbox"  ng-model="items.dragIn"  name="isDragIn" ng-true-value="1" ng-false-value="0">部门名称带入文书
        </div>
        <div class="col-xs-3"></div>
      </div>
    </div>


    <div class="form-group">
      <div class="row row_margin" >
        <div class="col-xs-3 text-right">
          <label><span class="xing">*</span><span class="xing"></span>具体地址</label>
        </div>
        <div class="col-xs-6">
          <input type="text" maxlength="100" class="form-control input-sm" ng-model="address" name="address" placeholder="请输入地址">
        </div>
        <div class="col-xs-3"></div>
        <span class="Prompt_message" ng-bind="addressErrorMessage"></span>
      </div>
    </div>
    <div class="clear"></div>
    <div class="form-group" ng-class="{'has-error':netErrorMessage}">
      <span class="Prompt_message" ng-bind="netErrorMessage"></span>
    </div>
  </form>
</div>
<div class="modal-footer">
  <div class="row">
    <div class="col-xs-3"></div>
    <div class="col-xs-6 btn_center">
      <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="cancel()">取消</button>
      <button type="button" class="btn btn-primary save_btn" ng-click="ok()">保存</button>
    </div>
    <div class="col-xs-3"></div>
  </div>
</div>