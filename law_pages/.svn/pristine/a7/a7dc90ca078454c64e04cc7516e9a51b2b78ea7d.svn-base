<div>
  <div ui-tree class="node-level-1" style="height: 270px;overflow: auto">
    <ol ui-tree-nodes="" ng-model="showData" >
      <li ng-repeat="item in showData" ui-tree-node  class="node-level-2" >
        <div  title="{{item.name}}">
          <i ng-class="{'node-opened':item.opened,'node-close':!item.opened}" ng-show="item.childList"  ng-click="showOrhide(item,$event)" ></i>
          <span  ng-click="selectedCourt(item,$event)">{{item.name}}</span>
        </div>

        <ol ui-tree-nodes="" ng-model="item.childList" ng-show="item.opened" style="margin-left:19px;">
          <li ng-repeat="subItem in item.childList" ui-tree-node class="node-level-3">
            <div title="{{subItem.name}}" ng-show="subItem.childList.length>0">
              <i ng-class="{'node-opened':subItem.opened,'node-close':!subItem.opened}"  ng-click="showOrhide(subItem,$event)" ></i>
              <span  ng-click="selectedCourt(subItem,$event)">{{subItem.name}}</span>
            </div>
            <div title="{{subItem.name}}"  ng-click="selectedCourt(subItem,$event)" ng-show="subItem.childList.length == 0">
              <i class="node-no-child"  ></i>
              <span ng-click="selectedCourt(subItem,$event)">{{subItem.name}}</span>
            </div>

            <ol ui-tree-nodes="" ng-model="subItem.childList"  ng-show="subItem.opened" style="margin-left:25px;">
              <li ng-repeat="thirdItem in subItem.childList" ui-tree-node class="node-level-4">
                <div  title="{{subItem.name}}">
                  <i></i>
                  <span ng-click="selectedCourt(thirdItem,$event)">{{thirdItem.name}}</span>
                </div>
              </li>
            </ol>
          </li>
        </ol>
      </li>
    </ol>
  </div>
</div>
