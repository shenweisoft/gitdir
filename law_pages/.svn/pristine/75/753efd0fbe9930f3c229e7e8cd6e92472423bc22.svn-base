<div class="row" id="policeList">
    <div style="margin-bottom: 45px;">
        <div class="module-box bg-default">
            <div class="nav-line-left">
                <h4>
                    <span class="h1_line"></span>
                    查询交警案件
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="form-group col-xs-6">
                            <div class="col-lg-4 col-xs-5 text-right">
                                <label class=" control-label"><i>*</i> 事故认定书编号</label>
                            </div>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentNumberError}">
                                <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.accidentNumber" placeholder="请输入事故认定书编号">
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-lg-4 col-xs-5 text-right  control-label">
                                <span class="Required" >*</span>事故地点
                            </label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.cityCodeError}">
                                <input class="form-control" name="cityName"   id="cityNameInput" ng-model="police.accidentRegion"  ng-click="cityNameClick($event)" readonly/>
                                <div class="panel panel-default padding_none" id="cityNamebox" style="position:absolute;left:15px;z-index:999;width:93%; display: none;" ng-click="cityNameboxClick($event)">
                                    <div class="panel-body">
                                        <div js-tree="treeConfig" should-apply="true" tree="treeInstance" ng-model="cityRegion" tree-events="select_node:selectAdmin" class="pre-scrollable"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 事故详细地点</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentAddressError}">
                                <input type="text" maxlength="100" class="form-control ng-pristine ng-valid" ng-model="police.accidentAddress" placeholder="请输入事故详细地点">
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-xs-6 row clearfix">
                            <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 事故时间</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentDateError}">
                                <div class="input-group">
                                    <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="police.accidentDate" is-open="police.accidentDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-default" ng-click="openAccidentDate($event, police)"><i class="glyphicon glyphicon-calendar"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-xs-6 row clearfix">
                            <div class="col-xs-12">
                                <div class="form-group">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 是否高速</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.highSpeedError}">
                                        <label class="radio-inline" ng-repeat="highSpeed in highSpeedList">
                                            <input type="radio" name="highSpeed" ng-model="police.highSpeed" ng-value="highSpeed.id" ng-checked="police.highSpeed == highSpeed.id">{{highSpeed.value}}
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-xs-6 row clearfix">
                            <div class="col-lg-4 col-xs-5 text-right">
                                <label class=" control-label"><i>*</i> 身份证号码</label>
                            </div>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.idNoError}">
                                <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.idNo" placeholder="请输入身份证号码">
                            </div>
                        </div>
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center">
                            <button class="btn btn-default" style="margin-right: 20px;" type="button" ng-click="clearPolice()">清空</button>
                            <button class="btn btn-brand" type="button" ng-click="queryDetail()">查询交警信息</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <div class="module-box bg-default">
            <div class="nav-line-left">
                <h4>
                    <span class="h1_line"></span>
                    人员信息登记
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix">
                        <div class="clearfix" person-flag="{{$index}}" style="border: 1px dashed transparent;padding-top: 15px;position:relative;margin-top: 15px;" ng-repeat="person in police.personInfoVOList" ng-mouseenter="showDeleteBtn($index, person, 'person')" ng-mouseleave="hideDeleteBtn($index, person, 'person')">
                            <button ng-show="person.showDeleBtn" class="btn btn-default btn-sm" style="position: absolute; right: 0;top: 0;" type="button" ng-click="handleDeletePerson($index)">删除</button>
                            <div class="form-group col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 当事人姓名</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.personNameError}">
                                    <input type="text" ng-disabled="police.personState == '1'" maxlength="20" class="form-control ng-pristine ng-valid" ng-model="person.personName" placeholder="请输入当事人姓名">
                                </div>
                            </div>
                            <div class="form-group col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 电话号码</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.telephoneError}">
                                    <input type="text" ng-disabled="police.personState == '1'" maxlength="20" class="form-control ng-pristine ng-valid" ng-model="person.telephone" placeholder="请输入手机号或座机号（座机号无法接收短信）">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 clearfix">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 身份证号码</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.idNoError}">
                                    <input type="text" ng-blur="checkIdentity(person, false)" ng-disabled="police.personState == '1'" class="form-control ng-pristine ng-valid" ng-model="person.idNo" placeholder="请输入身份证号">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 clearfix">
                                <div class="col-xs-12" style="height: 34px;">
                                    <label class="col-lg-4 col-xs-5 control-label"><span class="Required">*</span> 性别</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.sexError}">
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.personState == '1'" name={{"gender"+$index}} value="0" ng-checked="person.sex=='0'" ng-model="person.sex">男
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.personState == '1'" name={{"gender"+$index}} value="1" ng-checked="person.sex=='1'" ng-model="person.sex">女
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 居住地</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.residenceError}">
                                    <input type="text" ng-disabled="police.personState == '1'" maxlength="100" class="form-control ng-pristine ng-valid" ng-model="person.residence" placeholder="请输入居住地">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 出生日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.birthdayError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="person.birthday" is-open="person.birthdayIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" ng-disabled="police.personState == '1'" class="btn btn-default" ng-click="openPersonBirthday($event, person)"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!--以下为详细内容，展开显示-->
                            <div ng-show="person.isShowDetail">
                                <div class="form-group col-lg-6 col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">档案编号</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.archivesNoError}">
                                        <input type="text" ng-disabled="police.personState == '1'" maxlength="100" class="form-control ng-pristine ng-valid" ng-model="person.archivesNo" placeholder="请输入档案编号">
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">准驾车型</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.vehicleError}">
                                        <select class="form-control ng-pristine ng-valid" ng-disabled="police.personState == '1'" ng-model="person.vehicle">
                                            <option ng-selected="person.vehicle == x.id" ng-value="x.id" ng-repeat="x in vehicleList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">初次领证日期</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.certificateDateError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="person.certificateDate" is-open="person.certificateDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.personState == '1'" class="btn btn-default" ng-click="openCertificateDate($event, person)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">有效期始</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.validateStartDateError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="person.validateStartDate" is-open="person.validateStartDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.personState == '1'" class="btn btn-default" ng-click="openValidateStartDate($event, person)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">有效期止</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.validateEndDateError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="person.validateEndDate" is-open="person.validateEndDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.personState == '1'" class="btn btn-default" ng-click="openValidateEndDate($event, person)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">驾驶证期限</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.limitTimeError}">
                                        <input type="text" ng-disabled="police.personState == '1'" maxlength="5" class="form-control ng-pristine ng-valid" ng-model="person.limitTime" placeholder="请输入驾驶证期限">
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">驾驶证状态</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.driverLicenseStateError}">
                                        <select class="form-control ng-pristine ng-valid" ng-disabled="police.personState == '1'" ng-model="person.driverLicenseState">
                                            <option ng-selected="person.driverLicenseState == x.id" ng-value="x.id" ng-repeat="x in driverLicenseStateList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">机动车所有人</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.driverLicenseNameError}">
                                        <input type="text" ng-disabled="police.personState == '1'" maxlength="30" class="form-control ng-pristine ng-valid" ng-model="person.driverLicenseName" placeholder="请输入机动车所有人">
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">号牌种类</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.plateTypeError}">
                                        <select class="form-control ng-pristine ng-valid" ng-disabled="police.personState == '1'" ng-model="person.plateType">
                                            <option ng-selected="person.plateType == x.id" ng-value="x.id" ng-repeat="x in plateTypeList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">车辆型号</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.vehicleTypeError}">
                                        <select class="form-control ng-pristine ng-valid" ng-disabled="police.personState == '1'" ng-model="person.vehicleType">
                                            <option ng-selected="person.vehicleType == x.id" ng-value="x.id" ng-repeat="x in vehicleTypeList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-lg-4 col-xs-5 text-right">
                                        <label class=" control-label">使用性质</label>
                                    </div>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': person.usePropertyError}">
                                        <select class="form-control ng-pristine ng-valid" ng-disabled="police.personState == '1'" ng-model="person.useProperty">
                                            <option ng-selected="person.useProperty == x.id" ng-value="x.id" ng-repeat="x in usePropertyList">{{x.value}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-xs-6">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label">保险终止日期</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.endDateError}">
                                        <div class="input-group">
                                            <input type="text" class="form-control" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="person.endDate" is-open="person.endDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                            <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.personState == '1'" class="btn btn-default" ng-disabled="person.personState == '1'" ng-click="openEndDate($event, person)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-xs-12">
                                        <div class="form-group">
                                            <label class="col-lg-4 col-xs-5 control-label">是否酒后驾车</label>
                                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.drunkDrivingError}">
                                                <label class="radio-inline">
                                                    <input type="radio" ng-disabled="police.personState == '1'" name={{"drunk"+$index}} value="1" ng-model="person.drunkDriving">是
                                                </label>
                                                <label class="radio-inline">
                                                    <input type="radio" ng-disabled="police.personState == '1'" name={{"drunk"+$index}} value="2" ng-model="person.drunkDriving">否
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                    <div class="col-xs-12">
                                        <div class="form-group">
                                            <label class="col-lg-4 col-xs-5 control-label">是否逃逸</label>
                                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':person.isEscapeError}">
                                                <label class="radio-inline">
                                                    <input type="radio" ng-disabled="police.personState == '1'" name={{"escape"+$index}} value="1" ng-model="person.isEscape">是
                                                </label>
                                                <label class="radio-inline">
                                                    <input type="radio" ng-disabled="police.personState == '1'" name={{"escape"+$index}} value="2" ng-model="person.isEscape">否
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-xs-12 row clearfix">
                                    <div class="col-xs-12">
                                        <div class="form-group">
                                            <label class="col-lg-2 col-xs-2 control-label">当事人损伤情况</label>
                                            <div class="col-lg-10 col-xs-10">
                                                <textarea ng-disabled="police.personState == '1'" ng-class="{'has-error': person.personHitRemarkError}" maxlength="500" class="col-xs-12 text-box" name="remark" ng-model="person.personHitRemark"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-xs-12 row clearfix">
                                    <div class="col-xs-12">
                                        <div class="form-group">
                                            <label class="col-lg-2 col-xs-2 control-label">车物损失情况</label>
                                            <div class="col-lg-10 col-xs-10">
                                                <textarea ng-disabled="police.personState == '1'" ng-class="{'has-error': person.vehicleHitRemarkError}" maxlength="500" class="col-xs-12 text-box" name="remark" ng-model="person.vehicleHitRemark"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!--控制详情显示、隐藏 star-->
                            <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;padding: 10px 0 10px 0;margin-bottom: 0">
                                <a class="cur-pointer po-relative"  ng-click="handelDetailToggle(person)">{{person.isShowDetail? '收起更多': '展开更多'}} <i class="font iconfont">{{person.isShowDetail? '&#xe623;': '&#xe624;'}}</i></a>
                            </div>
                            <!--控制详情显示、隐藏 end-->

                        </div>

                        <div ng-show="police.personState != '1'" class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;margin-top: 15px;">
                            <button class="btn btn-default" type="button" style="margin-right: 20px;color: #3F65D6;" ng-click="appendPerson()">
                                <span class="glyphicon glyphicon-plus" style="font-size: 12px;"></span>
                                添加人员信息
                            </button>
                            <button ng-show="police.personInfoVOList.length > 0" class="btn btn-brand" style="margin-right: 20px;" type="button" ng-click="save12()">保存</button>
                            <button ng-show="police.personInfoVOList.length > 0" class="btn btn-brand" type="button" ng-click="submit12()">提交</button>
                        </div>

                    </div>
                </form>
            </div>
        </div>

        <div class="module-box bg-default" ng-if="step >= 2">
            <div class="nav-line-left">
                <h4>
                    <span class="h1_line"></span>
                    受案信息登记
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div ng-repeat="recipient in police.policeRecipientInfoVOList">
                            <div class="form-group col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 受案人</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': recipient.recipientNameError}">
                                    <input type="text" ng-disabled="police.recipientState == '1'" maxlength="30" class="form-control ng-pristine ng-valid" ng-model="recipient.recipientName" placeholder="请输入受案人姓名">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 案由</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': recipient.causeTypeError}">
                                    <select class="form-control ng-pristine ng-valid" ng-disabled="police.recipientState == '1'" ng-model="recipient.causeType">
                                        <option ng-selected="recipient.causeType == x.id" ng-value="x.id" ng-repeat="x in causeTypeList">{{x.value}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-xs-6">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 受案时间</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':recipient.recipientDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="recipient.recipientDate" is-open="recipient.recipientDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.recipientState == '1'" class="btn btn-default" ng-click="openRecipientDate($event, recipient)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                    </div>
                                </div>
                                <!--<div class="col-lg-8 col-xs-11">
                                    <div class="input-group" ng-class="{'has-error': law.recipient.recipientDateError}">
                                        <input type="text" readonly class="form-control" datetime-picker="yyyy-MM-dd" ng-model="law.recipient.recipientDate" enable-date="true" enable-time="true" is-open="law.recipient.recipientDateIsOpen" close-text="关闭" clear-text="清除" current-text="今天" />
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openRecipientDate($event, law.recipient)"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>-->
                            </div>
                            <div class="form-group col-lg-12 col-xs-12 row clearfix">
                                <div class="col-xs-12">
                                    <div class="form-group">
                                        <label class="col-lg-2 col-xs-5 control-label"><i>*</i>受案人意见</label>
                                        <div class="col-lg-10 col-xs-7">
                                            <textarea ng-disabled="police.recipientState == '1'" ng-class="{'has-error': recipient.recipientRemarkError}" maxlength="1000" class="col-xs-12 text-box" name="remark" ng-model="recipient.recipientRemark"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center" ng-show="police.recipientState != '1'">
                            <button class="btn btn-brand" style="margin-right: 20px;" type="button" ng-click="save12()">保存</button>
                            <button class="btn btn-brand" type="button" ng-click="submit12()">提交</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <div class="module-box bg-default" ng-if="step >= 3">
            <div class="nav-line-left">
                <h4>
                    <span class="h1_line"></span>
                    扣留车辆信息登记
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input" >
                    <div class="row clearfix ">
                        <div class="clearfix" vehicle-flag="{{$index}}" style="border: 1px dashed transparent;padding-top: 15px;position:relative;margin-top: 15px;" ng-repeat="vehicle in police.policeVehicleInfoVOList" ng-mouseenter="showDeleteBtn($index, vehicle, 'vehicle')" ng-mouseleave="hideDeleteBtn($index, vehicle, 'vehicle')">
                            <button ng-show="vehicle.showDeleBtn" class="btn btn-default btn-sm" style="position: absolute; right: 0;top: 0;" type="button" ng-click="handleDeleteVehicle($index)">删除</button>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 当事人姓名</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': vehicle.litigantNameError}">
                                    <select class="form-control ng-pristine ng-valid" ng-disabled="police.vehicleState == '1'" ng-model="vehicle.litigantName">
                                        <option ng-selected="vehicle.litigantName == x" ng-value="x" ng-repeat="x in vehicle.personList | querySelected: police.personInfoVOList: police.policeVehicleInfoVOList: vehicle">{{x}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 号牌号码</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': vehicle.plateNoError}">
                                    <input type="text" ng-disabled="police.vehicleState == '1'" maxlength="30" class="form-control ng-pristine ng-valid" ng-model="vehicle.plateNo" placeholder="请输入号牌号码">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <div class="col-xs-12">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 扣留行驶证</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':vehicle.detainDrivingLicenseError}">
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.vehicleState == '1'" name={{"license"+$index}} value="0" ng-model="vehicle.detainDrivingLicense">是
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.vehicleState == '1'" name={{"license"+$index}} value="1" ng-model="vehicle.detainDrivingLicense">否
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <div class="col-xs-12">
                                    <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 扣留车辆</label>
                                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':vehicle.detainVehicleError}">
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.vehicleState == '1'" name={{"vehicle"+$index}} value="0" ng-model="vehicle.detainVehicle">是
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" ng-disabled="police.vehicleState == '1'" name={{"vehicle"+$index}} value="1" ng-model="vehicle.detainVehicle">否
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="vehicle.detainVehicle == '0'">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 扣留日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':vehicle.detainDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="vehicle.detainDate" is-open="vehicle.detainDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.vehicleState == '1'" class="btn btn-default" ng-click="openDetainDate($event, vehicle)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="vehicle.detainVehicle == '0'">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 返还日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':vehicle.returnDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="vehicle.returnDate" is-open="vehicle.returnDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.vehicleState == '1'" class="btn btn-default" ng-click="openReturnDate($event, vehicle)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;margin-top: 15px;" ng-if="police.vehicleState != '1'">
                            <button class="btn btn-default" type="button" style="margin-right: 20px;color: #3F65D6;" ng-click="appendVehicle()">
                                <span class="glyphicon glyphicon-plus" style="font-size: 12px;"></span>
                                添加扣留车辆信息
                            </button>
                            <button ng-show="police.policeVehicleInfoVOList.length" class="btn btn-brand" type="button" ng-click="submitVehicle()">提交</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <div class="module-box bg-default" ng-if="step >= 3">
            <div class="nav-line-left">
                <h4>
                    <span class="h1_line"></span>
                    委托鉴定信息登记
                </h4>
            </div>
            <div class="module-content font-size13">
                <form role="form" class="form-horizontal form-input">
                    <div class="row clearfix ">
                        <div class="clearfix" appraise-flag="{{$index}}" style="border: 1px dashed transparent;padding-top: 15px;position:relative;margin-top: 15px;" ng-repeat="appraise in police.policeAppraiseInfoVOList" ng-mouseenter="showDeleteBtn($index, appraise, 'appraise')" ng-mouseleave="hideDeleteBtn($index, appraise, 'appraise')">
                            <button ng-show="appraise.showDeleBtn" class="btn btn-default btn-sm" style="position: absolute; right: 0;top: 0;" type="button" ng-click="handleDeleteAppraise($index)">删除</button>

                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 申请人姓名</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': appraise.applyerNameError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="appraise.applyerName" ng-disabled="police.appraiseState == '1'">
                                        <option ng-selected="appraise.applyerName == x" ng-value="x" ng-repeat="x in appraise.personList | querySelected: police.personInfoVOList: police.policeAppraiseInfoVOList: appraise">{{x}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 鉴定机构</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': appraise.accreditationBodiesError}">
                                    <input type="text" ng-disabled="police.appraiseState == '1'" maxlength="100" class="form-control ng-pristine ng-valid" ng-model="appraise.accreditationBodies" placeholder="请输入鉴定机构">
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 鉴定类型</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': appraise.identificationTypeError}">
                                    <select class="form-control ng-pristine ng-valid" ng-disabled="police.appraiseState == '1'"  ng-model="appraise.identificationType">
                                        <option ng-selected="appraise.identificationType == x.id" ng-value="x.id" ng-repeat="x in identificationTypeList">{{x.value}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 鉴定时间</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraise.identificationDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="appraise.identificationDate" is-open="appraise.identificationDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.appraiseState == '1'" class="btn btn-default" ng-click="openIdentificationDate($event, appraise)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 鉴定完成时间</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraise.identificationFinishDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="appraise.identificationFinishDate" is-open="appraise.identificationFinishDateDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                        <button type="button" ng-disabled="police.appraiseState == '1'" class="btn btn-default" ng-click="openIdentificationFinishDate($event, appraise)"><i class="glyphicon glyphicon-calendar"></i></button>
                                      </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;margin-top: 15px;" ng-if="police.appraiseState != '1'">
                            <button class="btn btn-default" type="button" style="margin-right: 20px;color: #3F65D6;" ng-click="appendAppraise()">
                                <span class="glyphicon glyphicon-plus" style="font-size: 12px;"></span>
                                添加委托鉴定信息
                            </button>
                            <button ng-show="police.policeAppraiseInfoVOList.length" class="btn btn-brand" type="button" ng-click="submitAppraise()">提交</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="panel-footer bg-default text-right bottom_button_box">
        <button class=" btn btn-brand" ng-click="endLawCase()" ng-show="step >= 3">补录完成</button>
        <button class=" btn btn-default" ng-click="closeLawCase()" style="margin-left: 10px; margin-right: 20px;">关闭</button>
    </div>
</div>