<div class="modal-header">
    <span ng-bind="addFlag ?'新增':'修改'"></span>
    <i class="iconfont pull-right icon-guanbi close" ng-click="cancel()"></i>
</div>
<table class=" modal-body height_box_d" >
    <div class="form_box" style="height:260px;">
        <form class="form-horizontal ng-pristine ng-valid">
            <div class="scroll-form-box" style="height: 260px;">
                <div class="form-group margin_top_popup row">
                    <label class="col-xs-offset-1 col-xs-2"><i class="Required">*</i> 域名名称</label>
                    <div class="col-xs-6" ng-class="{'has-error':domainNameErr}" style="position: relative;">
                        <input name="domainName" type="text" class="form-control input-sm ng-pristine ng-valid" ng-model="jyDomainDictionary.domainName"  ng-blur="checkName(true)" placeholder="请您输入域名名称">
                    </div>
                    <div class="error-message ng-binding" ng-bind="checkErr"></div>
                    <div class="error-message ng-binding" ng-bind="domainNameErr"></div>
                </div>
                <div class="form-group row">
                    <label class="col-xs-offset-1 col-xs-2"><i class="Required">*</i>对应内容</label>
                    <div class="col-xs-6" ng-class="{'has-error':contentErr}">
                        <input name="content" type="text" class="form-control input-sm ng-pristine ng-valid" ng-model="jyDomainDictionary.content" placeholder="请您输入对应内容">
                        <div class="error-message ng-binding" ng-bind="contentErr"></div>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-xs-offset-1 col-xs-2"><i class="Required white"></i> 内容描述</label>
                    <div class="col-xs-6" >
                        <input name="remark" type="text" class="form-control input-sm ng-pristine ng-valid" ng-model="jyDomainDictionary.remark" placeholder="请输入内容描述">
                    </div>
                </div>
            </div>
        </form>
    </div>
</table>
<div class="modal-footer">
    <div class="form-group">
        <div class="col-xs-offset-5 col-xs-1">
            <button type="submit" class="btn btn-default " ng-click="cancel()">取消</button>
        </div>
        <div class=" col-xs-1">
            <button type="submit" class="btn btn-brand bg-brand-color" ng-click="addDomain()" >保存</button>
        </div>

    </div>
</div>