angular.module('sbAdminApp').controller('chatSocketCtrl', function( $scope, $state,$log,$rootScope) {


    function Chat() {

        this.name = "";
        this.message = "";

    }

    $scope.chat = new Chat();

    var socket =  io.connect('http://192.168.223.150:9098');

    console.info('zhangsan');
    console.info(socket);


    socket.on('advert_info', function(data){
        console.info(data);

    });




    $scope.sendSocket = function(){

        socket.emit('advert_info', $scope.chat.name + $scope.chat.message);
    };
});