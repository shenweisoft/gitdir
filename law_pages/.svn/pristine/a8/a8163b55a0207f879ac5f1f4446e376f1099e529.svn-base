<div>
	<!--头部详细字段-->
	<div class="case-details-head bg-default font-size13">
		<div class="clearfix no-margin" ng-show="detailsHeadShow">
			<div class="avatar-content" ng-if="headDetail">
				<div class="row clearfix no-margin overflow-hidden po-relative" style="border-bottom: 1px solid #ddd;padding:15px 0;">
					<!-- 立案审批-->
					<div class="col-xs-11  no-padding-l " ng-class="{'border0': showFlag}" >
						<div class="col-xs-12">
							<h4>
								<span style="margin-right: 5px;" ng-repeat="applicant in secondApplicantArray | filter:{'personType': 0}">
									<b>上诉人(原审{{applicant.lawType == '1'? '原告':'被告'}})：{{applicant.personName}}</b>
									<small class="error-box mar-le-10" ng-show="secondIntanceInfo.state == '1001'">待审批</small>
									<small class="error-box mar-le-10" ng-show="secondIntanceInfo.state == '1002'">待分案</small>
									<small class="error-box mar-le-10" ng-show="secondIntanceInfo.state == '1003'">待排期</small>
								</span>
							</h4>
						</div>
						<div class="second-color">
							<div class="col-xs-4 hide-nowrap" ng-repeat="applicant in secondApplicantArray | filter:{'personType': 1}">被上诉人(原审{{applicant.lawType == '1'? '原告':'被告'}})：{{applicant.personName}}</div>
							<div class="col-xs-4" ng-if="secondIntanceInfo.state == '1001'">流水号：{{secondIntanceInfo.serialNo}}</div>
						</div>
					</div>
					<!-- 立案审批-->
					<!-- 分案 -->
					<div ng-if="secondIntanceInfo.state != '1001'" class="col-xs-11  no-padding-l second-color line-height2-0 " ng-class="{'border0': showFlag}" >
						<div class="col-xs-4 hide-nowrap">案由：{{secondIntanceInfo.reason | id2Text:factTypeList}}</div>
						<div class="col-xs-4 hide-nowrap col-xs-offset-8"></div>
						<div class="col-xs-4 hide-nowrap">案号：{{secondIntanceInfo.secondLawNo}}</div>
						<div class="col-xs-4">立案登记人：{{secondIntanceInfo.filingName}}</div>
						<div class="col-xs-4">登记日期：{{secondIntanceInfo.filingDate | stringDate | date: 'yyyy-MM-dd' }}</div>
						<div class="col-xs-4 hide-nowrap">承办部门：{{secondIntanceInfo.contractorDepartment}}</div>
						<div class="col-xs-4">立案审批人：{{secondIntanceInfo.checkName}}</div>
						<div class="col-xs-4">审批日期：{{secondIntanceInfo.checkDate}}</div>
					</div>
					<!-- 分案 -->
					<!-- 排期 -->
					<div ng-if="secondIntanceInfo.state != '1001' && secondIntanceInfo.state != '1002'" class="col-xs-11  no-padding-l second-color line-height2-0 " ng-class="{'border0': showFlag}" >
						<div class="col-xs-4 hide-nowrap">审判长：{{secondIntanceInfo.chiefJudgeName}}</div>
						<div class="col-xs-4">合议庭成员2：{{secondIntanceInfo.memberTwoName}}</div>
						<div class="col-xs-4">书记员：{{secondIntanceInfo.clerkName}}</div>
						<div class="col-xs-4 hide-nowrap">承办人：{{secondIntanceInfo.undertakerName}}</div>
						<div class="col-xs-4">合议庭成员1：{{secondIntanceInfo.memberOneName}}</div>
					</div>
					<!-- 排期 -->
					<!-- 庭审赔偿计算 -->
					<div ng-if="secondIntanceInfo.state != '1001' && secondIntanceInfo.state != '1002' && secondIntanceInfo.state != '1003'" class="col-xs-11  no-padding-l second-color line-height2-0 " ng-class="{'border0': showFlag}" >
						<div class="col-xs-4 hide-nowrap">庭次：{{secondIntanceInfo.courtNum}}</div>
						<div class="col-xs-4">法庭名称：{{secondIntanceInfo.courtName}}</div>
						<div class="col-xs-4">开庭时间：{{secondIntanceInfo.courtDate}}</div>
					</div>
					<!-- 庭审赔偿计算 -->
					<div class="col-xs-1 boder-left-pad" ng-if="!isDossier" onclick="javascript:history.go(-1)">
						<div class="back-box">
							<i class="iconfont icon-fanhui brand-color cur-pointer" ></i>
						</div>
					</div>
				</div>
				<div class="col-xs-12 po-relative" style="height: 48px;">
					<div class="avatar-icon-list line-height2 " style="line-height: 48px;">
						<div ng-show="!isDossier" class="brand-color cur-pointer text-decoration" ng-click="queryDossier()">
							<i class="iconfont icon-wenjianguanli"></i>查看卷宗
						</div>
						<div ng-show="!isEvidence" class="brand-color cur-pointer text-decoration" ng-click="queryEvidence()">
							<i class="iconfont icon-zheng"></i>查看证据
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="btn-recv cur-pointer" ng-class="{true:'btn-recv',false:'btn-pull'}[detailsHeadShow]" ng-click="detailsHeadShow=!detailsHeadShow"></div>
	</div>
</div>

