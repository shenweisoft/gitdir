<!--调取信息弹框-->
<div style="padding: 15px">
    <div class="form-group" style="border-bottom: 1px solid #ddd">
        <h4 class="col-xs-8" style="margin-left: -10px;">
            <span class="h1_line"></span>
            <span>事故基本信息</span>
        </h4>
        <i class="iconfont pull-right icon-guanbi close" ng-click="closeModal()"></i>
        <div style="clear: both"></div>
    </div>
    <div class="col-xs-12 form-group form-group-sm">
        <label class="col-xs-2 control-label" style="padding: 0 0 0 15px; line-height: 30px;"><span class="Required">*</span>事故认定编号</label>
        <div class="col-xs-4" ng-class="{'has-error':applicantInfo.accidentNumberError}">
            <input type="text" class="form-control ng-pristine ng-valid" ng-model="applicantInfo.accidentNumber" placeholder="请填写事故认定编号" maxlength="30">
        </div>
        <label class="col-xs-2 control-label" style="padding: 0 0 0 15px; line-height: 30px;"><span class="Required">*</span>当事人身份证号</label>
        <div class="col-xs-4" ng-class="{'has-error':applicantInfo.idNoError}">
            <input type="text" class="form-control ng-pristine ng-valid" ng-model="applicantInfo.idNo" placeholder="请填写当事人身份证号" maxlength="30">
        </div>
    </div>
    <div class="col-xs-12" style="text-align: center;">
        <button type="button" class="btn btn-brand" ng-click="handleApplicantInfo()" ng-disabled="isLoading">调取</button>
    </div>
    <br>
    <div class="col-xs-12" style="border-bottom: 1px solid #ddd" ng-show="policeInfoList.length > 0">
        <h4 class="col-xs-8" style="margin-left: -10px;">
            <i class="iconfont icon--faqisusong" style="color: #3f65d6;"></i>
            <span>事故人员信息</span>
        </h4>
    </div>

    <div ng-show="isLoading" class="col-xs-12 bg-default text-center">
        <div class="main-loading">
            <div class="loadEffect" style="margin-top: 20px;">
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
        <p style="line-height: 60px;">正在查询中，请耐心等待！</p>
    </div>

    <div style="float: left;height: 250px;overflow-y: auto;" ng-show="policeInfoList.length > 0">
        <!--人员信息框-->
        <div ng-repeat="police in policeInfoList" class="col-xs-5 policeInfoBox" ng-class="(police.userType == '0' || police.userType == '1')? 'active' : 'normal'">
            <div class="col-xs-12 mb-10" style="text-align: right;height: 17px;color: #3BB878;">
                <span ng-show="police.checkedUserType == '0'">已设置为申请人</span>
                <span ng-show="police.checkedUserType == '1'">已设置为被申请人</span>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">人员姓名：</div>
                <div class="col-lg-8 col-xs-8 ng-binding">{{police.xm}}</div>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">人员编号：</div>
                <div class="col-lg-8 col-xs-8 ng-binding">{{police.rybh}}</div>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">身份证号码：</div>
                <div class="col-lg-8 col-xs-8 ng-binding">{{police.sfzmhm}}</div>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">住址：</div>
                <div class="col-lg-8 col-xs-8 ng-binding" style="overflow: hidden;text-overflow: ellipsis;white-space: nowrap" title="{{police.zz}}">{{police.zz}}</div>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">联系电话：</div>
                <div class="col-lg-8 col-xs-8 ng-binding">{{police.dh}}</div>
            </div>
            <div class="col-xs-12 mb-10">
                <div class="col-lg-4 col-xs-4 ng-binding text-r" style="padding-left: 0">当事人身份：</div>
                <div class="col-lg-8 col-xs-8 ng-binding">
                    <select class="form-control" ng-model="police.userType">
                        <option value="">未选择</option>
                        <option ng-repeat="x in applicantInfoTypeList" ng-value="x.id">{{x.value}}</option>
                    </select>
                </div>
            </div>
            <i ng-show="police.userType == '0' || police.userType == '1'" class="iconfont icon-tupian-xuanzhong" style="position: absolute;right: 5px;bottom: 20px;font-size: 20px;color: #3BB878;"></i>
        </div>
    </div>
    <div class="col-xs-12" style="text-align: center;" ng-show="policeInfoList.length > 0">
        <button type="button" class="btn btn-brand" style="margin-top: 10px;" ng-click="fillData()">确认填充</button>
    </div>
    <div style="clear: both;"></div>
</div>
