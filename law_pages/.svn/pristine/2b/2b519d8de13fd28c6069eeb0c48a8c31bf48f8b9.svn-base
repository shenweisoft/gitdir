<div class="bg-default ng-scope" style="margin-left: -15px; margin-right: -15px;">
    <div class=" panel-heading">
        <div class="row">
            <div class="row" style="margin-bottom: 15px;">
                <div class="col-xs-12" style="text-align: center; line-height: 35px; font-size: 24px;">{{userName}}登录统计表</div>
            </div>
            <div class="col-xs-12">
                <div class="col-xs-1" style="font-size: 13px">
                    合计：{{queryUserLoginVO.pageTotal}}次
                </div>
            </div>
            <div class="col-xs-12">
                <table class="table table-striped-info table-fixed table-hover" ng-show="userLoginInfoList.length > 0">
                    <thead>
                        <tr>
                            <th class="col-xs-1">序号</th>
                            <th class="col-xs-5">登录时间</th>
                            <th class="col-xs-6">登录IP</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="x in userLoginInfoList">
                            <td>{{$index+1}}</td>
                            <td>{{x.createDate | stringDate | date: 'yyyy-MM-dd' }}</td>
                            <td>{{x.ipAddress}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="text-center"  ng-show="userLoginInfoList.length > 0">
                <pagination previous-text="<" next-text=">" first-text="<<" last-text=">>" items-per-page="queryUserLoginVO.pageSize" class="pagination-custom" total-items="queryUserLoginVO.pageTotal" ng-model="queryUserLoginVO.pageNo" ng-change="queryUserLoginInfo()" max-size="7" boundary-links="true" rotate="false"></pagination>
            </div>
            <div  class="text-center" ng-show="userLoginInfoList.length == 0">
                <img src="views/images/search_null.png" ><br>
                抱歉，未找到相关内容！
            </div>
            <div class="col-xs-12">
                <button type="button" class="btn btn-brand pull-right" ng-click="returnLoginStatistics()">返回</button>
            </div>
        </div>
    </div>
</div>