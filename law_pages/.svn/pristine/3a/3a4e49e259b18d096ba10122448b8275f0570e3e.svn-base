<div class="navbar-default sidebar" role="navigation">
  <div class="sidebar-nav navbar-collapse lg-sidebar">
    <ul class="nav in" id="side-menu">
      <!-- <sidebar-search></sidebar-search> -->
      <li style="width: 100%; height: 25px;"></li>
      <!--wangfeng code-->
      <!--<li ng-repeat="menu in menuList" ui-sref-active="active" ng-class="{active: menu.subMenu.length && collapseVar==menu.seq}">-->
      <li ng-repeat="menu in menuList" ui-sref-active="active" ng-class="{active: collapseVar==menu.seq}">
        <a href="" ng-if="!menu.subMenu.length" ui-sref="{{menu.menuUrl}}"><i class="" ng-class="menu.class"></i>{{menu.menuName}}</a>
        <a href="" ng-if="menu.subMenu.length" ng-click='check(menu.seq)'><i class="" ng-class="menu.class"></i>{{menu.menuName}}<span class="iconfont icon-xiangyou pull-right"></span></a>
        <ul ng-if="menu.subMenu.length" class="nav nav-second-level" collapse="collapseVar!=menu.seq">
          <li ng-repeat="subMenu in menu.subMenu" ui-sref-active="active" ng-hide="isSendAppraisal=='1' && (subMenu.seq=='12.1' || subMenu.seq=='11.4' || subMenu.seq=='11.1' || subMenu.seq=='11.2')" ng-class="{active: subMenu.subMenu.length && collapseVar==subMenu.seq}">
             <!--根据是否发起鉴定跳转-->
            <a href="" ng-if="!subMenu.subMenu.length && isSendAppraisal=='1' && (subMenu.seq=='12.2' || subMenu.seq=='11.3')" ui-sref="{{subMenu.menuUrl}}">历史查询</a>
            <a href="" ng-if="!subMenu.subMenu.length" ng-click="isSendAppraisalFn(subMenu.seq)" ui-sref="{{subMenu.menuUrl}}">{{subMenu.menuName}}</a>
            <a href="" ng-if="subMenu.subMenu.length" ng-click="isSendAppraisalFn(subMenu.seq)"><i class="" ng-class="subMenu.class"></i> {{subMenu.menuName}}<span class="iconfont icon-xiangyou pull-right"></span></a>
            <ul ng-if="subMenu.subMenu.length" class="nav nav-third-level" collapse="multiCollapseVar!=subMenu.seq">
              <li ng-repeat="subSubMenu in subMenu.subMenu" ng-class="{active: multiCollapseVar==subSubMenu.seq}">
                <a ui-sref="{{subSubMenu.menuUrl}}">{{subSubMenu.menuName}}</a>
              </li>
            </ul>
          </li>
        </ul>
      </li>
    </ul>
  </div>
  <div ng-show="citizen == MENU_CONSTANT.citizenShow"  class="Prosecution_btn_box" style="padding-top:60px;">
    <div style="margin:20px 50px;"><button class="btn btn_tj" style="padding:0 40px;height:36px;" ui-sref="dashboard.readNotice">我要调解</button></div>
    <div style="margin:20px 50px;"><button class="btn btn_qs" style="padding:0 40px;height:36px;" ui-sref="dashboard.sue_input({'type':'0'})">我要起诉</button></div>
    <div style="margin:20px 50px;"><button class="btn btn_ys" style="padding:0 40px;height:36px;" ui-sref="dashboard.sue_input({'type':'1'})">我要应诉</button></div>

  </div>
  <!-- 在窄屏的下的边框 -->
  <div class="xs-sidebar" ng-mouseleave="twoLevelMenuShow=false">
    <div class="two_level_menu" ng-show="twoLevelMenuShow" >
      <div ng-repeat="menu in menuList" ng-show="showSubMenu==menu.menuName" >
        <h4 class="boder_bottom">{{menu.menuName}}</h4>
        <div>
          <div ng-repeat="subMenu in menu.subMenu" ui-sref="{{subMenu.menuUrl}}" class="two_level_menu_e" ui-sref-active="xs-active">{{subMenu.menuName}}</div>
        </div>
        <div style="clear: both;"></div>
      </div>
    </div>
    <dl class="sidebar_e" ng-repeat="menu in menuList" ng-mouseenter="twoLevelMenu(menu.menuName);">
      <dt><i class="iconfont" ng-class="menu.class"></i></dt>
      <dd>{{menu.menuName}}</dd>
    </dl>
    <div ng-show="citizen == MENU_CONSTANT.citizenShow"  class="Prosecution_btn_box_xs">
      <div style="margin:70px 10px 10px 10px; "><button class="btn btn-warning" style="padding:0 20px; height: 36px;" ui-sref="dashboard.sue_input({'type':'0'})">起诉</button></div>
      <div style="margin:10px;"><button class="btn btn-success" style="padding:0 20px; height: 36px;" ui-sref="dashboard.sue_input({'type':'1'})">应诉</button></div>
    </div>
  </div>



</div>