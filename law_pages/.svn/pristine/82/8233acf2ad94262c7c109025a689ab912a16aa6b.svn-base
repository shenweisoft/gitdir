<div class="module-box bg-default" style="padding-bottom: 10px;" id="policeTab1">
    <div class="module-content font-size13">
        <form role="form" class="form-horizontal form-input">
            <div class="row clearfix ">
                <div class="form-group col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 事故编号</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.accidentCodeError}">
                        <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.accidentCode" placeholder="请输入事故编号" maxlength="100">
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                    <label class="col-lg-4 col-xs-5 text-right control-label"><i>*</i> 事故时间</label>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentInfo.accidentTimeError}">
                        <div class="input-group" style="width: 100%;">
                            <input class="Wdate form-control form_datetime_2 my97datePicker" ng-model="police.accidentInfo.accidentTime" onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss',readOnly:true, startDate:'%y-%M-01 00:00:00', qsEnabled: false, onpicked:function(dp) {my97dateClick(dp)}})" />
                        </div>
                    </div>
                </div>
                <div class="form-group col-xs-6">
                    <label class="col-lg-4 col-xs-5 text-right control-label">
                        <span class="Required" >*</span>事故地点
                    </label>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentInfo.accidentPlaceError}">
                        <input class="form-control" name="cityName" id="cityNameInput" ng-model="police.accidentInfo.accidentPlace"  ng-click="cityNameClick($event)" readonly/>
                        <div class="panel panel-default padding_none" id="cityNamebox" style="position:absolute;left:15px;z-index:999;width:93%; display: none;" ng-click="cityNameboxClick($event)">
                            <div class="panel-body">
                                <div js-tree="treeConfig" should-apply="true" tree="treeInstance" ng-model="cityRegion" tree-events="select_node:selectAdmin" class="pre-scrollable"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--<div class="form-group col-lg-6 col-xs-6 row clearfix">
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 是否高速</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentInfo.highSpeedError}">
                                <label class="radio-inline" ng-repeat="highSpeed in highSpeedList">
                                    <input type="radio" name="highSpeed" ng-model="police.accidentInfo.highSpeed" ng-value="highSpeed.id" ng-checked="police.accidentInfo.highSpeed == highSpeed.id">{{highSpeed.value}}
                                </label>
                            </div>
                        </div>
                    </div>
                </div>-->
                <div class="form-group col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label">事故详细地址</label>
                    </div>
                    <div class="col-lg-8 col-xs-7">
                        <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.accidentAddress" maxlength="200" int-only>
                    </div>
                </div>
                <div class="form-group col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 死亡人数</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.deathNumberError}">
                        <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.deathNumber" maxlength="11" int-only>
                    </div>
                </div>
                <div class="form-group col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 受伤人数</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.injuredNumberError}">
                        <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.injuredNumber" maxlength="11" int-only>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 事故类型</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.accidentTypeError}">
                        <select class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.accidentType">
                            <option ng-selected="police.accidentInfo.accidentType == x.id" ng-value="x.id" ng-repeat="x in accidentTypeList">{{x.value}}</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 现场形态</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.sceneError}">
                        <select class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.scene">
                            <option ng-selected="police.accidentInfo.scene == x.id" ng-value="x.id" ng-repeat="x in sceneList">{{x.value}}</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-lg-12 col-xs-12 row clearfix">
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label class="col-lg-2 col-xs-5 control-label"><i>*</i>简要案情</label>
                            <div class="col-lg-10 col-xs-7">
                                <textarea ng-class="{'has-error': police.accidentInfo.briefCaseError}" maxlength="1000" class="col-xs-12 text-box" name="remark" ng-model="police.accidentInfo.briefCase"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 直接财产损失</label>
                    </div>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error': police.accidentInfo.propertyLossError}">
                        <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.propertyLoss" placeholder="请输入直接财产损失" float-only>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6">
                    <div class="col-lg-4 col-xs-5 text-right">
                        <label class=" control-label"><i>*</i> 事故认定原因</label>
                    </div>
                    <div class="col-lg-3 col-xs-3" ng-class="{'has-error': police.accidentInfo.accidentCauseCodeError}">
                        <input type="text" maxlength="50" class="form-control ng-pristine ng-valid" ng-blur="queryAccidentCauseRemark(police.accidentInfo.accidentCauseCode, accidentCauseRemarkList, police.accidentInfo, 'accidentCauseRemark')" ng-model="police.accidentInfo.accidentCauseCode" placeholder="请输入原因编号">
                    </div>
                    <div class="col-lg-5 col-xs-4" ng-class="{'has-error': police.accidentInfo.accidentCauseRemarkError}">
                        <select class="form-control ng-pristine ng-valid" ng-model="police.accidentInfo.accidentCauseRemark" ng-change="filterAccidentCauseRemark(police.accidentInfo.accidentCauseRemark, accidentCauseRemarkList, police.accidentInfo, 'accidentCauseCode')">
                            <option ng-selected="police.accidentInfo.accidentCauseRemark == x.name" ng-value="x.name" ng-repeat="x in accidentCauseRemarkList">{{x.code}}：{{x.name}}</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                    <label class="col-lg-4 col-xs-5 text-right control-label"><i>*</i> 现场勘查日期</label>
                    <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentInfo.surveyDateError}">
                        <div class="input-group">
                            <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-change="changeSurDate(police)" ng-readonly="true" ng-model="police.accidentInfo.surveyDate" is-open="police.surveyDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-default" ng-click="openSurveyDate($event, police)"><i class="glyphicon glyphicon-calendar"></i></button>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-xs-6 row clearfix">
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label class="col-lg-4 col-xs-5 text-right  control-label"><span class="Required">*</span> 是否检验鉴定</label>
                            <div class="col-lg-8 col-xs-7" ng-class="{'has-error':police.accidentInfo.isAppraisalError}">
                                <label class="radio-inline">
                                    <input type="radio" name="isAppraisal" ng-model="police.accidentInfo.isAppraisal" ng-checked="police.accidentInfo.isAppraisal == '1'" value="1" ng-click="co.addAppraisalInfo(true)">是
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="isAppraisal" ng-model="police.accidentInfo.isAppraisal" ng-checked="police.accidentInfo.isAppraisal == '0'" value="0">否
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

    <!--检验鉴定信息-->
    <div style="border: 1px solid #ddd;" ng-show="police.accidentInfo.isAppraisal == '1'">
        <div class="nav-line-left" style="background-color: #f2f2f2;">
            <h4 class="col-lg-8 col-xs-8" style="padding: 0 20px;margin: 0;">
                <span class="h1_line"></span>
                检验鉴定信息
            </h4>
            <div class="col-lg-4 col-xs-4">
                <button type="button" class="btn btn-brand pull-right" style="margin-top: 6px;opacity: 0.8;" ng-click="co.addAppraisalInfo()">
                    <span style="font-size: 12px;" class="glyphicon glyphicon-plus"></span>
                    新增
                </button>
            </div>
        </div>
        <div class="module-content font-size13">
            <form role="form" class="form-horizontal form-input">
                <!--此处需要循环-->
                <div style="border-bottom: 1px dashed #ddd" ng-repeat="appraisalInfo in police.appraisalInfoList">
                    <div class="row clearfix ">
                        <div class="form-group col-lg-12 col-xs-12 row clearfix" style="margin-bottom: 0">
                            <div class="col-xs-12">
                                <div class="form-group">
                                    <label class="col-lg-2 col-xs-2 text-right  control-label"><span class="Required">*</span> 检验鉴定类型</label>
                                    <div class="col-lg-10 col-xs-10" ng-class="{'has-error':appraisalInfo.appraisalTypeError}">
                                        <label class="radio-inline" ng-repeat="type in policeAppraisalType" style="padding-top: 0" ng-checked="handleAppraisalType(appraisalInfo)">
                                            <input type="radio" name="{{'policeAppraisalType'+$parent.$index}}" ng-value="type.id" ng-model="appraisalInfo.appraisalType" ng-disabled="appraisalInfo.appraisalState >= '2'">{{type.value}}
                                        </label>
                                        <label class="radio-inline" style="margin-left: -15px;padding-top: 0" ng-class="{'has-error':appraisalInfo.appraisalTypeRemarkError}">
                                            <input type="text" maxlength="50" class="form-control input-inline ng-pristine ng-valid" ng-model="appraisalInfo.appraisalTypeRemark" ng-disabled="appraisalInfo.appraisalState >= '2' || appraisalInfo.appraisalType != '4'">
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label"><i>*</i> 检测鉴定机构</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': appraisalInfo.appraisalOrgIdError}">
                                    <select class="form-control ng-pristine ng-valid" ng-model="appraisalInfo.appraisalOrgId" ng-change="getValueById(appraisalInfo.appraisalOrgId, appraisalInfo, 'appraisalOrgName', appraisalMechanismList, 'relationOrgName')" ng-disabled="appraisalInfo.appraisalState >= '2'">
                                        <option ng-selected="appraisalInfo.appraisalOrgId == x.relationOrgId" ng-value="x.relationOrgId" ng-repeat="x in appraisalMechanismList | filter:{category: '#03'}">{{x.relationOrgName}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 委托日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.entrustTimeError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-change="changeEntTime(appraisalInfo,police)" ng-readonly="true" ng-model="appraisalInfo.entrustTime" is-open="appraisalInfo.entrustTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openEntrustTime($event, appraisalInfo)" ng-disabled="appraisalInfo.appraisalState >= '2'"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i>约定完成日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.promiseTimeError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-change="changePromTime(appraisalInfo)" ng-readonly="true" ng-model="appraisalInfo.promiseTime" is-open="appraisalInfo.promiseTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openPromiseTime($event, appraisalInfo)" ng-disabled="appraisalInfo.appraisalState >= '2'"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6">
                                <div class="col-lg-4 col-xs-5 text-right">
                                    <label class=" control-label">当事人</label>
                                </div>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error': appraisalInfo.partyError}">
                                    <select class="form-control" ng-model="appraisalInfo.party" ng-options="party.policePersonInformation.id as party.policePersonInformation.userName for party in police.jpoliceVehicVO" ng-disabled="appraisalInfo.appraisalState >= '3'">
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="appraisalInfo.appraisalState >= '2'">
                                <div class="col-xs-12">
                                    <div>
                                        <label class="col-lg-4 col-xs-5 text-right  control-label"><i>*</i> 是否生效</label>
                                        <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.isEffectiveError}">
                                            <label class="radio-inline">
                                                <input type="radio" name="{{'isEffective_'+$index}}" ng-model="appraisalInfo.isEffective" ng-checked="appraisalInfo.isEffective == '0'" value="0" ng-disabled="appraisalInfo.appraisalState > '2'">是
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="{{'isEffective_'+$index}}" ng-model="appraisalInfo.isEffective" ng-checked="appraisalInfo.isEffective == '1'" value="1" ng-disabled="appraisalInfo.appraisalState > '2'">否
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="appraisalInfo.appraisalState >= '2'">
                                <label class="col-lg-4 col-xs-5 text-right  control-label"><i ng-show="appraisalInfo.isEffective == '0'">*</i> 生效日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.effectiveDateError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="appraisalInfo.effectiveDate" is-open="appraisalInfo.effectiveDateIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openEffectiveDate($event, appraisalInfo)" ng-disabled="appraisalInfo.appraisalState > '2'"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="appraisalInfo.appraisalState >= '2'">
                                <label class="col-lg-4 col-xs-5 text-right  control-label">委托完成时限</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.entrustFinishTimeError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="appraisalInfo.entrustFinishTime" is-open="appraisalInfo.entrustFinishTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openEntrustFinishTime($event, appraisalInfo, $index)" ng-disabled="true"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-xs-6 row clearfix" ng-show="appraisalInfo.appraisalState >= '2'">
                                <label class="col-lg-4 col-xs-5 text-right  control-label">实际完成日期</label>
                                <div class="col-lg-8 col-xs-7" ng-class="{'has-error':appraisalInfo.completeTimeError}">
                                    <div class="input-group">
                                        <input type="text" class="form-control form_datetime_2" max-date="today" datepicker-popup="yyyy-MM-dd" ng-readonly="true" ng-model="appraisalInfo.completeTime" is-open="appraisalInfo.completeTimeIsOpen" datepicker-options="{startingDay: 1}" close-text="关闭" clear-text="清除" current-text="今天"/>
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openCompleteTime($event, appraisalInfo)" ng-disabled="true"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xs-12" ng-show="appraisalInfo.appraisalState>='2'">
                                <div class="col-lg-2 col-xs-2 text-right">
                                    <label class=" control-label">检测鉴定报告</label>
                                </div>
                                <!--<div class="col-lg-1 col-xs-1" style="margin-right: 20px;">
                                    <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ngf-multiple="true" ngf-pattern=".doc,.docx,image/*"  accept="image/gif,image/jpeg,image/jpg,image/png,image/svg,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document" ngf-select="addEvidence($files, appraisalInfo)"  ng-click="checkflash()" ng-model="files">
                                        <i class="iconfont icon-shangchuan brand-color" style="color: #fff;"></i>
                                        上传
                                    </button>
                                </div>
                                <div class="col-lg-1 col-xs-1" style="font-size: 16px;border-left: 1px solid #E4E4E4;padding-left: 30px;" ng-if="appraisalInfo.enclosureInfoList.length > 0">
                                    <label class="checkbox-inline">
                                        <input type="checkbox" ng-checked="appraisalInfo.isAllChecked" ng-click="handleAllChecked($event, appraisalInfo.enclosureInfoList, appraisalInfo)">全选
                                    </label>
                                </div>
                                <div class="col-lg-1 col-xs-1" ng-if="appraisalInfo.enclosureInfoList.length > 0 && appraisalInfo.enclosureArr.length">
                                    <button type="button" class="btn btn-brand pull-right" style="opacity: 0.8;position: relative;top: -2px;" ng-click="deleteEnclosure(appraisalInfo)">
                                        删除
                                    </button>
                                </div>-->
                            </div>
                            <div class="form-group col-xs-12" ng-show="appraisalInfo.appraisalState>='2'">
                                <div class="col-lg-2 col-xs-2 text-right"></div>
                                <div class="col-lg-10 col-xs-10">
                                    <ul class="evidences">
                                        <li ng-repeat="enclosureInfo in appraisalInfo.enclosureInfoList" ng-click="handleEnclosure(enclosureInfo, appraisalInfo.enclosureInfoList, appraisalInfo)">
                                            <!--<i ng-show="appraisalInfo.appraisalState < '3'" class="{{enclosureInfo.isChecked? 'icon-active':'icon-normal'}}"></i>-->
                                            <!--<img ng-src="{{enclosureInfo.fileType == '0'? imageAddress+enclosureInfo.path : 'views/images/police-evidence-default.png'}}" alt="">-->
                                            <img ng-show="enclosureInfo.fileType == '0'" ng-src="{{imageAddress+enclosureInfo.path}}" ng-click="zoomImage(bigPictureUrl+enclosureInfo.path)">
                                            <a ng-click="downloadFile(enclosureInfo)">
                                                <img ng-show="enclosureInfo.fileType == '1'" ng-src="{{'views/images/police-evidence-default.png'}}" alt="">
                                                <img ng-show="enclosureInfo.fileType == '2'" ng-src="{{'views/images/police-evidence-default2.png'}}" alt="">
                                            </a>
                                            <p>{{enclosureInfo.oldname}}</p>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="form-group col-lg-12 col-xs-12 row clearfix" style="text-align: center;">
                                <button style="margin-right: 20px;" type="button" class="btn btn-brand" ng-show="police.appraisalInfoList.length > 1 && appraisalInfo.appraisalState<'3'" ng-click="handleDeleteAppraisalInfo($index, appraisalInfo)">删除</button>
                                <!--<button style="margin-right: 20px;" type="button" class="btn btn-brand" ng-show="appraisalInfo.appraisalState == '3'" ng-click="handleSendEntrust(appraisalInfo, '4')">确认生效</button>-->
                                <button type="button" class="btn btn-brand" ng-show="appraisalInfo.appraisalState == '0'" ng-click="handleSendEntrust(appraisalInfo, '1')">发送委托</button>
                                <button type="button" class="btn btn-brand" disabled ng-show="appraisalInfo.appraisalState == '1'">已发送委托</button>
                                <button type="button" class="btn btn-brand" ng-show="appraisalInfo.appraisalState == '2'" ng-click="handleSendEntrust(appraisalInfo, '3')">完成委托</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<!--遮罩层-上传证据期间-->
<div ng-show="fileUploading" class="file-upload-masker"></div>
<p   ng-show="fileUploading" class="file-upload-masker_text">上传中，请稍等...</p>
<div ng-show="fileUploading" id='box'>
    <div id="ball1"></div>
    <div id="ball2"></div>
    <div id="ball3"></div>
</div>

<div id="big_img_box">
    <div class="big_img_box_bj"></div>
    <div class="big_img_box_img_box">
        <div id="big_img_box_img_f" style="position: absolute;">
