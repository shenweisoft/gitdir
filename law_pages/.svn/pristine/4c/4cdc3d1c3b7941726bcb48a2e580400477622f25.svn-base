              <div class="input-group">
                <input type="text" class="form-control" datepicker-popup="yyyy-MM-dd" ng-model="lawCase.closeCaseDate" is-open="adjustDateOpened"
                       min-date="2010-01-01" max-date="'2020-06-22'" datepicker-options="dateOptions" close-text="Close"/>
