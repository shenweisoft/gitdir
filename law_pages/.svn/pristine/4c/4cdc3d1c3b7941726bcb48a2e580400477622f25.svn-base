    </div>
  </div>

  <!--调取交警信息弹框-->
  <div class="row police-info-box getPolice" ng-show="getPoliceBow">
    <p class="title">调取事故信息<i class="iconfont pull-right icon-guanbi close" style="margin-top: 2px;" ng-click="policeBowClose()"></i></p>
    <div class="row" style="padding: 15px;margin: 0 30px 40px;">
      <div class="col-xs-6 form-group-sm">
        <label class="col-xs-12 control-label" style="font-weight: normal;"><span class="Required">*</span>事故责任认定书编号</label>
      </div>
      <div class="col-xs-6 form-group-sm">
        <label class="col-xs-12 control-label" style="font-weight: normal;"><span class="Required">*</span>事故地点</label>
      </div>
      <div class="col-xs-6 form-group form-group-sm">
        <div class="col-xs-12" ng-class="{'has-error':police.accidentNumberError}">
          <input type="text" class="form-control ng-pristine ng-valid" ng-model="police.accidentNumber" placeholder="请输入">
        </div>
      </div>
      <div class="col-lg-6 col-xs-6 form-group form-group-sm" ng-class="{'has-error':police.accidentRegionError}">
        <input class="form-control" name="cityName" id="cityNameInput" ng-model="police.accidentRegion"  ng-click="cityNameClick($event)" readonly/>
        <div class="panel panel-default padding_none" id="cityNamebox" style="position:absolute;left:15px;z-index:999;width:93%; display: none;" ng-click="cityNameboxClick($event)">
          <div class="panel-body">
            <div js-tree="treeConfig" should-apply="true" tree="treeInstance" ng-model="cityRegion" tree-events="select_node:selectAdmin" class="pre-scrollable"></div>
          </div>
        </div>
      </div>
      <div class="col-xs-12 form-group-sm">
        <label class="col-xs-12 control-label" style="font-weight: normal;"><span class="Required">*</span>是否高速</label>
      </div>
      <div class="col-lg-12 col-xs-12 form-group-sm form-group" style="padding-left:30px;" ng-class="{'has-error':police.highSpeedError}">
        <label class="radio-inline" ng-repeat="highSpeed in highSpeedList">
          <input type="radio" name="highSpeed" ng-model="police.highSpeed" ng-value="highSpeed.id" ng-checked="police.highSpeed == highSpeed.id">{{highSpeed.value}}
        </label>
      </div>

      <div class="col-xs-12">
        <!-- card_active  选中状态 -->
        <div class="card_box1 cur-pointer " ng-repeat="applicant in lawCase.applicantArray" ng-click="selectApplicant(applicant)" ng-class="{'card_active':applicant.idNo == police.idNo }" ng-if="applicant.idType== '0'">
          <img src="views/images/active_fw_r2_c2.png">
          <p >姓名：<span>{{applicant.personName}}</span></p>
          <p>性别：<span>{{applicant.sex | id2Text:sexList}}</span></p>
          <p>证件号码：<span>{{applicant.idNo}}</span></p>
          <p>出生日期：<span>{{applicant.birthDay | stringDate|date: 'yyyy-MM-dd' }}</span></p>
          <p>手机号码：<span>{{applicant.telephone}}</span></p>
          <p>住所地：{{applicant.residence}}</p>
        </div>
      </div>

    </div>
    <div class="policeBowFooter">
      <button class=" btn btn-default" style="margin-right: 15px;" ng-click="policeBowClose()">取消</button>
      <button class=" btn btn-brand" ng-click="getPoliceDetail()">查询</button>
    </div>
  </div>

  <div class="police-info-masker" ng-show="checkPoliceBow || getPoliceBow"></div>
</div>